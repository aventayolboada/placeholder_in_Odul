  <stext>
    <IU n="1">льэ киэйэ-н чуөльэ-д,</IU>
    <IU n="2">омнии старый мон-у-т,</IU>
    <IU n="3">шоромо-лэк льэ-льэл-у-л.</IU>
    <IU n="4">(1.5)</IU>
    <IU n="5">шаман-эк.</IU>
    <IU n="6">(0.8)</IU>
    <IU n="7">дьэ тамун,</IU>
    <IU n="8">(1.1)</IU>
    <IU n="9">ирк-и-н,</IU>
    <IU n="10">льэ-льэл,</IU>
    <IU n="11">пөги-ньэ-льэл,</IU>
    <IU n="12">(1.3)</IU>
    <IU n="13">ньаасьпэн-гэн мидол-аа-льэл-ӈи,</IU>
    <IU n="14">...</IU>
    <IU n="15">чуөльэ-д омнии саханэ.</IU>
    <IU n="16">(0.6)</IU>
    <IU n="17">таа-т...мон-и,</IU>
    <IU n="18">дьиибэ-н миэстэ-к льэ-л.</IU>
    <IU n="19">(0.5)</IU>
    <IU n="20">таа шаал-дэ йуө-дэ-гэ ирк-и-н мархиль-эк.</IU>
    <IU n="21">(0.4)</IU>
    <IU n="22">шаа-н кисил-гэ модо-т йахтэ-т модо-й.</IU>
    <IU n="23">(1.0)</IU>
    <IU n="24">дьэ.</IU>
    <IU n="25">(0.4)</IU>
    <IU n="26">тамун-гэлэ льэ-льэл-у-м,</IU>
    <IU n="27">(0.5)</IU>
    <IU n="28">тудэ льэ-гэлэ.</IU>
    <IU n="29">(0.6)</IU>
    <IU n="30">истриэла-гэлэ мин-дэллэ табуд-э дьэ.</IU>
    <IU n="31">айии-льэл-у-м.</IU>
    <IU n="32">(1.8)</IU>
    <IU n="33">айии-дэ-гэ,</IU>
    <IU n="34">лосьил-эк тамун льэ-гэт йиэ-р-эй-льэл-у-л.</IU>
    <IU n="35">(1.5)</IU>
    <IU n="36">[тии] —</IU>
    <IU n="37">(0.6)</IU>
    <IU n="38">тии өнмэ-лэ таа-т шоҕо-шо-льэл-у-м.</IU>
    <IU n="39">(0.5)</IU>
    <IU n="40">дьэ таа-т аӈсьии-т эди-ӈ ань-ньэ-й.</IU>
    <IU n="41">(0.4)</IU>
    <IU n="42">та-ӈ пөгиль-ги старый сразу лэй-тэй-льэл-у-м.</IU>
    <IU n="43">(0.5)</IU>
    <IU n="44">што таа-т кудэ-й мон-у-т.</IU>
    <IU n="45">(0.6)</IU>
    <IU n="46">дьэ йуө-дэллэ пойй-иэ-т кэсьии-льэл-ӈаа,</IU>
    <IU n="47">(0.9)</IU>
    <IU n="48">кэсьии-дэллэ дьэ йалҕи-ньаа-льэл.</IU>
    <IU n="49">(1.0)</IU>
    <IU n="50">та-ӈ старый.</IU>
    <IU n="51">(1.1)</IU>
    <IU n="52">йалҕи-ньэ-т,</IU>
    <IU n="53">дьэ таа-т мол-льэл,</IU>
    <IU n="54">ньань-уу-л-бэн мархиль-эк мо-ллэ мөд-и-сь,</IU>
    <IU n="55">(0.6)</IU>
    <IU n="56">айии-ну-млэ мо-ллэ мөд-и-сь.</IU>
    <IU n="57">(0.7)</IU>
    <IU n="58">аӈнумэ та-ӈ мархиль йалҕи-н,</IU>
    <IU n="59">льэ-ӈ ньань-уу-л-бэн-диэ,</IU>
    <IU n="60">(0.7)</IU>
    <IU n="61">льэ-льэл-мэлэ,</IU>
    <IU n="62">(0.8)</IU>
    <IU n="63">эмтэ-дэй-льэл-мэлэ,</IU>
    <IU n="64">(0.7)</IU>
    <IU n="65">таа-т кэл-дэллэ тудэ пөгиль эмтэ-дэй-льэл-мэлэ,</IU>
    <IU n="66">тамун ань-ньэ-дэ-гэ,</IU>
    <IU n="67">(0.8)</IU>
    <IU n="68">эдуөн пиэ-д албэ-гэ.</IU>
    <IU n="69">(1.0)</IU>
    <IU n="70">шаманьиха пиэ-д албэ-гэ.</IU>
    <IU n="71">(0.8)</IU>
    <IU n="72">палка-лэк эгэ-тиэ-льэл-мэлэ,</IU>
    <IU n="73">спиэчиальный шаал-эк.</IU>
    <IU n="74">(1.5)</IU>
    <IU n="75">та-ӈ шаал,</IU>
    <IU n="76">чистэ чуму статуй-лэк эгэ-тиэ-льэл-мэлэ.</IU>
    <IU n="77">(0.8)</IU>
    <IU n="78">та-ӈ статуй,</IU>
    <IU n="79">(1.3)</IU>
    <IU n="80">ти-ӈ идьии,</IU>
    <IU n="81">(0.2)</IU>
    <IU n="82">лэньинград-кэ льэ-й.</IU>
  </stext>
