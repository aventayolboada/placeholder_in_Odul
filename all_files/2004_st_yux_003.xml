    <fileDesc>
      <title>The shaman Staryj</title>
      <sourceDesc>author="Nikolaeva &amp; Mayer" yearpub="2004" location="https://www.sgr.fi/yukaghir/start1.html"</sourceDesc>
      <textDesc>variety="Kolyma Yukaghir" genre="story" daterec="1990" locrec="Unknown"</textDesc>
