  <stext>
    <IU n="1">чуөльэ-д омнии шоромо-пул,</IU>
    <IU n="2">(0.8)</IU>
    <IU n="3">мэт хаахаа эсьиэ-дэ,</IU>
    <IU n="4">(1.8)</IU>
    <IU n="5">эмдьэ льэ-льэл-ӈи.</IU>
    <IU n="6">(5.0)</IU>
    <IU n="7">тудаа,</IU>
    <IU n="8">(1.5)</IU>
    <IU n="9">тудэ-л-дэ нингэ-й уө-ньэ-й,</IU>
    <IU n="10">(2.1)</IU>
    <IU n="11">та-ӈ уөр-пэ-п-ки дьиэтэҕэ,</IU>
    <IU n="12">(2.0)</IU>
    <IU n="13">мэт эмэй та-ӈ-пэ-ньэ ньэ шоромо-нь-у-т,</IU>
    <IU n="14">(0.2)</IU>
    <IU n="15">чумут эй-р-оо-ӈи.</IU>
    <IU n="16">(1.5)</IU>
    <IU n="17">дьэ таа йообии эй-рэ-т,</IU>
    <IU n="18">хон-у-йии-т чуөльэ-д омнии титэ.</IU>
    <IU n="19">(0.8)</IU>
    <IU n="20">пугэ-мэ,</IU>
    <IU n="21">наар эксиль-э,</IU>
    <IU n="22">хаарбэс-э.</IU>
    <IU n="23">(0.7)</IU>
    <IU n="24">унуӈ-гэт унуӈ-ӈин ньэ маало-ҕудэ эй-рэ-т титтэ-л ай.</IU>
    <IU n="25">(1.3)</IU>
    <IU n="26">эг-у-жу-нун-льэл-ӈи.</IU>
    <IU n="27">(1.9)</IU>
    <IU n="28">хорхонаӈиль-гэн,</IU>
    <IU n="29">(1.3)</IU>
    <IU n="30">нэлучоо-пэ-гэн,</IU>
    <IU n="31">(1.1)</IU>
    <IU n="32">көжэдэн-пэ-гэн,</IU>
    <IU n="33">ньаасьпэн-пэ-гэн,</IU>
    <IU n="34">(0.7)</IU>
    <IU n="35">таа-т тии чаахадан-гэн,</IU>
    <IU n="36">(1.1)</IU>
    <IU n="37">паҕул пад-у-т мон-у-т.</IU>
    <IU n="38">(2.4)</IU>
    <IU n="39">омо-сьэ сьиилэ-й,</IU>
    <IU n="40">(0.3)</IU>
    <IU n="41">пулут оо-льэл.</IU>
    <IU n="42">(0.5)</IU>
    <IU n="43">иӈдьии-ньэ-й пулут.</IU>
    <IU n="44">(1.9)</IU>
    <IU n="45">таӈдаа,</IU>
    <IU n="46">(0.8)</IU>
    <IU n="47">мит шоромо-пул,</IU>
    <IU n="48">(0.5)</IU>
    <IU n="49">ниӈиӈиэл-пэ —</IU>
    <IU n="50">долганов,</IU>
    <IU n="51">(1.3)</IU>
    <IU n="52">солнцэв мон-у-т.</IU>
    <IU n="53">(2.0)</IU>
    <IU n="54">шалугиин мон-у-т.</IU>
    <IU n="55">(1.5)</IU>
    <IU n="56">тамун-пэ чистэ чуму шоромо-нь-у-т,</IU>
    <IU n="57">(1.0)</IU>
    <IU n="58">льэ-й,</IU>
    <IU n="59">(0.3)</IU>
    <IU n="60">пулут.</IU>
    <IU n="61">(2.3)</IU>
    <IU n="62">льэ,</IU>
    <IU n="63">адил-ӈоот куд-у-й-оо-л уө-ӈоо-ӈоот ам-дэ-дин льэ-т тамун-гэлэ эл пунду-л-бэд-эк.</IU>
    <IU n="64">(1.6)</IU>
    <IU n="65">андаа,</IU>
    <IU n="66">(1.5)</IU>
    <IU n="67">хаахаа нумө-ньэ-й миэстэ-гэ йаха-иили.</IU>
    <IU n="68">шоромо,</IU>
    <IU n="69">(0.5)</IU>
    <IU n="70">льэ-гэ,</IU>
    <IU n="71">(0.4)</IU>
    <IU n="72">льэ-ну-льэл,</IU>
    <IU n="73">(2.1)</IU>
    <IU n="74">өйльэ-л-оо-дэ-йнэ мол-льэл.</IU>
    <IU n="75">(1.5)</IU>
    <IU n="76">тудэ льэ-ги маҕил-ги,</IU>
    <IU n="77">(0.8)</IU>
    <IU n="78">[йаа-н],</IU>
    <IU n="79">(2.5)</IU>
    <IU n="80">атах-у-н нугэн-ньэ-й,</IU>
    <IU n="81">чомоолбэн хаар-э,</IU>
    <IU n="82">(0.4)</IU>
    <IU n="83">льэ-й-бэд-эк,</IU>
    <IU n="84">(0.5)</IU>
    <IU n="85">йоло-ҕудэ лааҕ-э-т,</IU>
    <IU n="86">таа-т-миэ-дьоон моро-дэллэ мон-у-т,</IU>
    <IU n="87">(1.6)</IU>
    <IU n="88">диннаах маҕил-эк.</IU>
    <IU n="89">(0.8)</IU>
    <IU n="90">туөн моро-дэллэ андаа льэ-дэ-гэ.</IU>
    <IU n="91">(0.8)</IU>
    <IU n="92">аӈиль-дэ-гэ мод-аа-ну-льэл.</IU>
    <IU n="93">(0.2)</IU>
    <IU n="94">йоло-ҕудэ.</IU>
    <IU n="95">(1.2)</IU>
    <IU n="96">хожидэлбэ-гэн мой-т.</IU>
    <IU n="97">(0.9)</IU>
    <IU n="98">тудэ чобинэ-гэлэ.</IU>
    <IU n="99">(1.6)</IU>
    <IU n="100">чобинэ-лэ,</IU>
    <IU n="101">(0.6)</IU>
    <IU n="102">таа эй-тэ-сь-ну-льэл-у-м.</IU>
    <IU n="103">(2.7)</IU>
    <IU n="104">кэл-дэ-йнэ тамун тудэ чобинэ-лэ эл уу-шэ-т бойсьо.</IU>
    <IU n="105">(1.5)</IU>
    <IU n="106">эл киэ-сьоон йоло-ҕудэ пэшшэй-м,</IU>
    <IU n="107">(0.5)</IU>
    <IU n="108">таа нуг-у-нну-льэл-у-м.</IU>
    <IU n="109">льэ-ӈидэ —</IU>
    <IU n="110">(1.1)</IU>
    <IU n="111">льэ-й —</IU>
    <IU n="112">(0.5)</IU>
    <IU n="113">льэ-гэлэ,</IU>
    <IU n="114">(4.9)</IU>
    <IU n="115">чаҕ-у-йии-л льэ-гэлэ,</IU>
    <IU n="116">(0.3)</IU>
    <IU n="117">хаахаа-гэлэ,</IU>
    <IU n="118">(1.1)</IU>
    <IU n="119">тамун-гэлэ,</IU>
    <IU n="120">(1.3)</IU>
    <IU n="121">тудэ льэ-лэ,</IU>
    <IU n="122">(0.4)</IU>
    <IU n="123">патрон-э,</IU>
    <IU n="124">(0.2)</IU>
    <IU n="125">мон-нун-ну-ӈи йук-оо-дьэ унуӈ-диэ-гэ ...</IU>
    <IU n="126">(0.8)</IU>
    <IU n="127">патрон абу-ш-тэллэ,</IU>
    <IU n="128">(0.7)</IU>
    <IU n="129">табуд-э пай-нун-ну-льэл.</IU>
    <IU n="130">(0.7)</IU>
    <IU n="131">миэкэ эл йох-тэ-сьуөн.</IU>
    <IU n="132">(1.2)</IU>
    <IU n="133">йоуҕо-гэт мин-дэллэ ти-ӈидэ кэсьии-нун-ну-м.</IU>
    <IU n="134">(0.6)</IU>
    <IU n="135">йоло-ҕудэ аа-сьии-нун-ну-м.</IU>
    <IU n="136">(1.2)</IU>
    <IU n="137">чомоолбэн-гэлэ,</IU>
    <IU n="138">(0.9)</IU>
    <IU n="139">өндьэ-гэлэ.</IU>
    <IU n="140">(0.5)</IU>
    <IU n="141">өрдьэ-дэ-гэн льэ-ллэ,</IU>
    <IU n="142">(1.1)</IU>
    <IU n="143">тамун-гэлэ чуму пай-иэ-т,</IU>
    <IU n="144">хаарбэс-льэл-у-м йообии та-ӈидэ элэйии-ну-льэл-у-м.</IU>
    <IU n="145">(1.8)</IU>
    <IU n="146">ирк-и-дьэ,</IU>
    <IU n="147">(1.5)</IU>
    <IU n="148">хойл нумө уй-аа-гэ параа-пэ-дэ-гэ,</IU>
    <IU n="149">(1.9)</IU>
    <IU n="150">та-ӈ пулут,</IU>
    <IU n="151">(2.2)</IU>
    <IU n="152">ай уө-ӈоо-льэл-тэ-й.</IU>
    <IU n="153">(1.5)</IU>
    <IU n="154">хойл нумө-н самый,</IU>
    <IU n="155">(1.5)</IU>
    <IU n="156">йоӈсьо чом-оо-л йоӈсьо-дэ-йлэ,</IU>
    <IU n="157">(0.9)</IU>
    <IU n="158">орп-иэ-ӈилэ-бэн.</IU>
    <IU n="159">(1.3)</IU>
    <IU n="160">таа криэст,</IU>
    <IU n="161">(0.4)</IU>
    <IU n="162">эгэ-тэ-сь-ну-льэл-бэн льэ-п-тэ-йлэ,</IU>
    <IU n="163">(1.1)</IU>
    <IU n="164">йаа-н кунэль,</IU>
    <IU n="165">(0.2)</IU>
    <IU n="166">молҕоилэк-у-н миэтрэ-нь-и йугул шаал-эк,</IU>
    <IU n="167">(0.3)</IU>
    <IU n="168">надо-ӈоо-ну-льэл.</IU>
    <IU n="169">(1.3)</IU>
    <IU n="170">тамун-гэлэ,</IU>
    <IU n="171">молҕаа-н шоромо-ӈоот.</IU>
    <IU n="172">(1.2)</IU>
    <IU n="173">йоӈсь-иэ-ль-иэ-льэл-ӈаа.</IU>
    <IU n="174">(1.3)</IU>
    <IU n="175">йаха-пэ йаа-н шоромо оо-льэл-ӈи,</IU>
    <IU n="176">(0.2)</IU>
    <IU n="177">оду-пэ йаа-н шоромо оо-льэл-ӈи.</IU>
    <IU n="178">(1.0)</IU>
    <IU n="179">эксиль-э.</IU>
    <IU n="180">(2.4)</IU>
    <IU n="181">дьэ таа-т хоӈ-ӈи-дэ-гэ,</IU>
    <IU n="182">(1.0)</IU>
    <IU n="183">тии мит унуӈ-гэн,</IU>
    <IU n="184">чаахадан унуӈ-гэн хон-у-т,</IU>
    <IU n="185">(0.3)</IU>
    <IU n="186">ойдоҕо-ткэ мон-у-т,</IU>
    <IU n="187">(0.4)</IU>
    <IU n="188">пуйд-оо-л ойдоҕо-ткэ.</IU>
    <IU n="189">(1.3)</IU>
    <IU n="190">йэвражка лэй-дии-бэд-эк.</IU>
    <IU n="191">(2.6)</IU>
    <IU n="192">таа ирк-и-н шаал ход-оо-льэл-тэ-й,</IU>
    <IU n="193">йуө-льэл-ӈи.</IU>
    <IU n="194">тамун,</IU>
    <IU n="195">(1.6)</IU>
    <IU n="196">хам-у-н,</IU>
    <IU n="197">(1.6)</IU>
    <IU n="198">мэндьэ-к мон-у-т,</IU>
    <IU n="199">(1.3)</IU>
    <IU n="200">йуө-дэллэ,</IU>
    <IU n="201">(1.4)</IU>
    <IU n="202">туөд-эк ти-ӈ льэ-т-у-л моӈ-ӈи.</IU>
    <IU n="203">(4.4)</IU>
    <IU n="204">ти-ӈ чох-тэллэ йиэ-рэ-ш-лу-кэнэ туөн льэ-т-и.</IU>
    <IU n="205">(1.0)</IU>
    <IU n="206">андаа хойл нумө,</IU>
    <IU n="207">(0.2)</IU>
    <IU n="208">уй-дин таа-т-миэ шаал нуу-йэ шоромо,</IU>
    <IU n="209">(1.0)</IU>
    <IU n="210">хой-н нумө,</IU>
    <IU n="211">(0.1)</IU>
    <IU n="212">льэ-гэ,</IU>
    <IU n="213">уөль-гэ таа.</IU>
    <IU n="214">(1.4)</IU>
    <IU n="215">[өйльэ] —</IU>
    <IU n="216">(0.2)</IU>
    <IU n="217">өйльэ-дэ-йнэ таа пөнь-ии-ну-льэл-ӈаа,</IU>
    <IU n="218">айии,</IU>
    <IU n="219">(0.4)</IU>
    <IU n="220">поӈдо-лэ эн-рэ-гэ,</IU>
    <IU n="221">(0.6)</IU>
    <IU n="222">[тадии-ну-льэл-ӈи] —</IU>
    <IU n="223">тадии-ну-льэл-ӈи.</IU>
    <IU n="224">(1.0)</IU>
    <IU n="225">таа-т-бэн оо-гэ-н.</IU>
    <IU n="226">(1.3)</IU>
    <IU n="227">таа нул-льэл-ӈаа,</IU>
    <IU n="228">(2.0)</IU>
    <IU n="229">оду-пэ,</IU>
    <IU n="230">(0.8)</IU>
    <IU n="231">киэйиэ-н кэл-дэллэ,</IU>
    <IU n="232">(1.1)</IU>
    <IU n="233">унуӈ-гэ чай оожэ-дин модо-ӈи-дэ-гэ,</IU>
    <IU n="234">йаха-пэ кэл-дэллэ,</IU>
    <IU n="235">(0.5)</IU>
    <IU n="236">та-ӈ шаал будиэ льэ-льэл-ӈи,</IU>
    <IU n="237">(3.1)</IU>
    <IU n="238">йаха-льэл-ӈи эндии льэ-льэл-ӈи,</IU>
    <IU n="239">(0.9)</IU>
    <IU n="240">мол-льэл-ӈи миннэ оду-пэ-гэт,</IU>
    <IU n="241">(0.7)</IU>
    <IU n="242">оно-т,</IU>
    <IU n="243">мин-дэллэ,</IU>
    <IU n="244">(0.7)</IU>
    <IU n="245">көуд-эй-тэ-й мон-у-т.</IU>
    <IU n="246">(1.4)</IU>
    <IU n="247">...,</IU>
    <IU n="248">(0.5)</IU>
    <IU n="249">та-ӈ мэт пулут хаахаа,</IU>
    <IU n="250">(1.3)</IU>
    <IU n="251">иисьэ-дэ-гэт мин-дэллэ,</IU>
    <IU n="252">(1.8)</IU>
    <IU n="253">унуӈ лаӈи сьиилэ-сь-нь-у-т аа-сьии-льэл-у-м.</IU>
    <IU n="254">пай-ай-дэллэ.</IU>
    <IU n="255">(2.0)</IU>
    <IU n="256">таа-т,</IU>
    <IU n="257">(0.3)</IU>
    <IU n="258">тудэ-л пуйлэ-ӈин хол-льэл.</IU>
    <IU n="259">(2.9)</IU>
    <IU n="260">дьэ таа-т тудэ кэнмэ-пул-ӈин,</IU>
    <IU n="261">(1.2)</IU>
    <IU n="262">таасилэ,</IU>
    <IU n="263">(0.2)</IU>
    <IU n="264">чай оожэ-ллэ эл омду-дэллэ.</IU>
    <IU n="265">(1.2)</IU>
    <IU n="266">уөр-пэ.</IU>
    <IU n="267">(1.4)</IU>
    <IU n="268">чай оожэ-ллэ,</IU>
    <IU n="269">льэ-ллэ йиэрэмэ-ш-тэллэ,</IU>
    <IU n="270">(0.9)</IU>
    <IU n="271">оожии лаӈи-н льэ-й,</IU>
    <IU n="272">(0.7)</IU>
    <IU n="273">пөмэ-гэ-дэй-тэ-й.</IU>
    <IU n="274">(1.8)</IU>
    <IU n="275">[тии-нэ йаха-ллэ] —</IU>
    <IU n="276">тии-нэ йаха-дэллэ эди-ӈ [шаал-дэ] —</IU>
    <IU n="277">(0.1)</IU>
    <IU n="278">шаал-гэлэ,</IU>
    <IU n="279">(0.4)</IU>
    <IU n="280">ти-ӈ чох-тэ-й эндии туөн мит шаал мон-у-т.</IU>
    <IU n="281">(1.0)</IU>
    <IU n="282">таа-т,</IU>
    <IU n="283">(0.9)</IU>
    <IU n="284">мэндьэ-лэ пөнь-ии-льэл-ӈаа.</IU>
    <IU n="285">(0.4)</IU>
    <IU n="286">таӈдаа,</IU>
    <IU n="287">(1.5)</IU>
    <IU n="288">тамун йолаа,</IU>
    <IU n="289">та-ӈ шаал лаӈи мэ та-ӈ йаха-пэ,</IU>
    <IU n="290">(0.2)</IU>
    <IU n="291">уй-ль-иэ-льэл-ӈи.</IU>
    <IU n="292">(2.4)</IU>
    <IU n="293">эл йуукэ эл кудэ-льэл-ӈи.</IU>
    <IU n="294">(1.7)</IU>
    <IU n="295">тамун-гэ та-ӈ титтэ шаал-гэлэ,</IU>
    <IU n="296">таа йох-таа-льэл-ӈаа,</IU>
    <IU n="297">(1.8)</IU>
    <IU n="298">йах-т-оо-пэ-дэ йолаа андаа,</IU>
    <IU n="299">(0.8)</IU>
    <IU n="300">титт-и-н,</IU>
    <IU n="301">(2.2)</IU>
    <IU n="302">хойл льэ,</IU>
    <IU n="303">мон-у-т.</IU>
    <IU n="304">(0.6)</IU>
    <IU n="305">хойл нумө поӈдо-ги таа,</IU>
    <IU n="306">пунт мон-тэ-йэ,</IU>
    <IU n="307">(0.7)</IU>
    <IU n="308">...,</IU>
    <IU n="309">(1.0)</IU>
    <IU n="310">мон-ну-ги,</IU>
    <IU n="311">(0.3)</IU>
    <IU n="312">льэ-лэк,</IU>
    <IU n="313">(4.4)</IU>
    <IU n="314">... мон-нун-ну-ӈи.</IU>
    <IU n="315">(1.9)</IU>
    <IU n="316">та-ӈ льэ-дэ-гэт,</IU>
    <IU n="317">(0.7)</IU>
    <IU n="318">тадии-льэл-ӈаа,</IU>
    <IU n="319">(0.6)</IU>
    <IU n="320">аасьэ-лэ,</IU>
    <IU n="321">(0.4)</IU>
    <IU n="322">уй-ль-оо-дэ йолаа-т.</IU>
    <IU n="323">(2.0)</IU>
    <IU n="324">таа хон-тэ-ллэ,</IU>
    <IU n="325">(0.9)</IU>
    <IU n="326">хой-н нумө-н уөль-гэ таа,</IU>
    <IU n="327">(1.3)</IU>
    <IU n="328">пөнь-ии-льэл-ӈаа.</IU>
    <IU n="329">(1.2)</IU>
    <IU n="330">тудэ саам дьиэтэҕэ ньаасьпэн-гэ,</IU>
    <IU n="331">(0.9)</IU>
    <IU n="332">андаа эл хол-ии,</IU>
    <IU n="333">(0.9)</IU>
    <IU n="334">угурчэраа лон-чии-т эй-рэ-т,</IU>
    <IU n="335">таа омон льэ-льэл,</IU>
    <IU n="336">(1.4)</IU>
    <IU n="337">йоӈж-уу-льэл.</IU>
    <IU n="338">(1.6)</IU>
    <IU n="339">табуд-э чэм-иэ-сь.</IU>
    <IU n="340">(0.2)</IU>
    <IU n="341">всьо.</IU>
  </stext>
