    <IU n="813">(0.3)</IU>
    <IU n="814">та-ӈ куд-дэ-т.</IU>
    <IU n="815">(1.2)</IU>
    <IU n="816">лөудэ-т мон-у-т.</IU>
    <IU n="817">(3.4)</IU>
    <IU n="818">тии-т мит,</IU>
    <IU n="819">(1.2)</IU>
