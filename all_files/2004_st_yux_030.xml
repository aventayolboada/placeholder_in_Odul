    <IU n="776">(0.7)</IU>
    <IU n="777">мон-у-т.</IU>
    <IU n="778">(1.3)</IU>
    <IU n="779">мэт ирк-и-дьэ айии.</IU>
    <IU n="780">(10.9)</IU>
