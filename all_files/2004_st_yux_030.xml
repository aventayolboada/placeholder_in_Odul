    <IU n="761">(1.1)</IU>
    <IU n="762">мит лаӈи-дэ кэл-у-ну-дэ.</IU>
    <IU n="763">(1.5)</IU>
    <IU n="764">киндьэ-гэт оожии-гэн,</IU>
    <IU n="765">(1.6)</IU>
