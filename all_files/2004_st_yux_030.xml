    <IU n="832">(2.4)</IU>
    <IU n="833">йод-ай-ӈи-к.</IU>
    <IU n="834">(0.9)</IU>
    <IU n="835">таасилэ иӈ-ии-ӈаа.</IU>
    <IU n="836">(1.6)</IU>
