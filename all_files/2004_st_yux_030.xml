    <IU n="745">(2.9)</IU>
    <IU n="746">йэд-уу-йии.</IU>
    <IU n="747">(0.9)</IU>
    <IU n="748">чумут эл льэн,</IU>
    <IU n="749">(2.2)</IU>
    <IU n="750">иисьэ-ги йэд-у-й-льэл.</IU>
    <IU n="751">(2.9)</IU>
