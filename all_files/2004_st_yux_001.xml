  <stext>
    <IU n="1">таа-т,</IU>
    <IU n="2">(1.5)</IU>
    <IU n="3">оду-н хаӈисьэ льэ-льэл,</IU>
    <IU n="4">(2.7)</IU>
    <IU n="5">льэ-гэ,</IU>
    <IU n="6">(0.7)</IU>
    <IU n="7">тэрикэ-ньэ-й,</IU>
    <IU n="8">уө-ньэ-й,</IU>
    <IU n="9">ирк-и-н мархи-ньаа-льэл,</IU>
    <IU n="10">модо-льэл.</IU>
    <IU n="11">(0.4)</IU>
    <IU n="12">модо-до-го,</IU>
    <IU n="13">(0.6)</IU>
    <IU n="14">ажуу-лэк кэл-льэл-у-л,</IU>
    <IU n="15">(0.6)</IU>
    <IU n="16">мэндьэ-лэк мөд-ии-льэл-у-л,</IU>
    <IU n="17">(0.6)</IU>
    <IU n="18">кэрэкэ хаӈисьэ-к кэл-тэ-й мон-у-т.</IU>
    <IU n="19">(2.9)</IU>
    <IU n="20">табун мөд-ии-дэ-гэ,</IU>
    <IU n="21">(1.0)</IU>
    <IU n="22">та-ӈ кэл-оо-дэ можуу-дэ-гэ нумө-ньэ-й,</IU>
    <IU n="23">(0.3)</IU>
    <IU n="24">нумө-ньэ-т льэ-л.</IU>
    <IU n="25">(1.4)</IU>
    <IU n="26">кэрэкэ хаӈисьэ-ги киэ-сь,</IU>
    <IU n="27">(0.5)</IU>
    <IU n="28">кэл-дэллэ йоул-у-сь-у-м,</IU>
    <IU n="29">тии оду-н хаӈисьэ хол-льэ.</IU>
    <IU n="30">(0.7)</IU>
    <IU n="31">мон-дэ-гэ —</IU>
    <IU n="32">мон-и,</IU>
    <IU n="33">(0.5)</IU>
    <IU n="34">льэ-к,</IU>
    <IU n="35">тии нумө-дьэ-к,</IU>
    <IU n="36">мэт аль-аа,</IU>
    <IU n="37">угуйэ мэт тэт-у-л хон-тэ-т мон-и,</IU>
    <IU n="38">(0.3)</IU>
    <IU n="39">тамуӈ-ӈин.</IU>
    <IU n="40">(1.3)</IU>
    <IU n="41">таа-т мон-дэ-гэ,</IU>
    <IU n="42">(0.5)</IU>
    <IU n="43">оду-н хаӈисьэ йоуло-мэ,</IU>
    <IU n="44">(0.4)</IU>
    <IU n="45">тудэ хаӈсаа-гэлэ,</IU>
    <IU n="46">мин-дэ-гэ,</IU>
    <IU n="47">(1.1)</IU>
    <IU n="48">йургуу-дэ-йлэ тоҕ-у-м,</IU>
    <IU n="49">(0.9)</IU>
    <IU n="50">тамун-дэ-гэ,</IU>
    <IU n="51">(0.9)</IU>
    <IU n="52">льэ-ллэ —</IU>
    <IU n="53">кудэ-дэллэ угуйэ-л-мэ,</IU>
    <IU n="54">(0.7)</IU>
    <IU n="55">та-ӈ кэрэкэ хаӈисьэ-ньэ дьэ кэб-эй-ӈи.</IU>
    <IU n="56">(1.0)</IU>
    <IU n="57">кэб-эй-дэллэ,</IU>
    <IU n="58">тудэ льэ-гэлэ —</IU>
    <IU n="59">тудэ чоҕойо-гэлэ,</IU>
    <IU n="60">(0.6)</IU>
    <IU n="61">йоӈжо-м.</IU>
    <IU n="62">(1.0)</IU>
    <IU n="63">пөнь-аа-ш-у-м.</IU>
    <IU n="64">(1.1)</IU>
    <IU n="65">таа-т хон-у-т,</IU>
    <IU n="66">(0.9)</IU>
    <IU n="67">хон-дэ-гэ,</IU>
    <IU n="68">(0.8)</IU>
    <IU n="69">эди-ӈ оду-н,</IU>
    <IU n="70">(0.7)</IU>
    <IU n="71">хаӈисьэ мон-и,</IU>
    <IU n="72">чаачаа</IU>
    <IU n="73">(1.7)</IU>
    <IU n="74">йуул пэшшэй-гэ мон-и.</IU>
    <IU n="75">(1.7)</IU>
    <IU n="76">таа-т-миэ-й миэстэ-к,</IU>
    <IU n="77">(0.9)</IU>
    <IU n="78">нум-мэлэ,</IU>
    <IU n="79">(1.9)</IU>
    <IU n="80">кэрэкэ-н хаӈисьэ-гэлэ,</IU>
    <IU n="81">(0.4)</IU>
    <IU n="82">льэ-гэ мот-тэ-м.</IU>
    <IU n="83">(0.8)</IU>
    <IU n="84">[ньанмэ] —</IU>
    <IU n="85">(0.7)</IU>
    <IU n="86">ньанмэ-н-дьэ миэстэ-гэ мот-тэ-м,</IU>
    <IU n="87">(2.1)</IU>
    <IU n="88">тудэ саам,</IU>
    <IU n="89">(2.2)</IU>
    <IU n="90">[эл] —</IU>
    <IU n="91">эл шаа-н миэстэ-гэ мад-аа-й,</IU>
    <IU n="92">аль-дэ-гэ.</IU>
    <IU n="93">(1.2)</IU>
    <IU n="94">мад-аа-дэллэ,</IU>
    <IU n="95">(0.4)</IU>
    <IU n="96">тудэ хаӈсаа аа-сьии-дэллэ,</IU>
    <IU n="97">тудэ хаӈсаа-гэлэ таа-т иб-и-шэ-м.</IU>
    <IU n="98">(0.7)</IU>
    <IU n="99">хаӈсаа-ги төб-оо-й.</IU>
    <IU n="100">(0.8)</IU>
    <IU n="101">таасилэ мон-и,</IU>
    <IU n="102">(0.5)</IU>
    <IU n="103">оо,</IU>
    <IU n="104">таа-т тудэ чоҕойодабут-кэлэ мидь-у-м,</IU>
    <IU n="105">(0.4)</IU>
    <IU n="106">чоҕойо йоӈж-аа-льэл-дьэ мон-и.</IU>
    <IU n="107">оо чаачаа мон-и,</IU>
    <IU n="108">мэт чоҕойо йоӈж-аа-льэл-дьэ,</IU>
    <IU n="109">(0.5)</IU>
    <IU n="110">тэт-кэ льэ-й,</IU>
    <IU n="111">(0.4)</IU>
    <IU n="112">тамун-гэ,</IU>
    <IU n="113">тудэ чоҕойо-гэлэ аа-сьии-дэллэ,</IU>
    <IU n="114">(0.3)</IU>
    <IU n="115">кэрэкэ ... хаӈисьэ мон-и,</IU>
    <IU n="116">тадии-гэ-к.</IU>
    <IU n="117">(0.9)</IU>
    <IU n="118">тадии-дэ-гэ таа-т,</IU>
    <IU n="119">(0.5)</IU>
    <IU n="120">кургом,</IU>
    <IU n="121">(0.9)</IU>
    <IU n="122">йуө-дэллэ,</IU>
    <IU n="123">(0.3)</IU>
    <IU n="124">бэйдьиэ тии таа йуө-дэллэ,</IU>
    <IU n="125">(0.5)</IU>
    <IU n="126">мон-и,</IU>
    <IU n="127">(0.5)</IU>
    <IU n="128">чаачаа тэт йэкльиэ-н,</IU>
    <IU n="129">(0.4)</IU>
    <IU n="130">ньанмэ шэльгэ-дэй-дэллэ кэй-к.</IU>
    <IU n="131">(1.6)</IU>
    <IU n="132">[кэрэкэ] —</IU>
    <IU n="133">(0.7)</IU>
    <IU n="134">кэрэкэ-н,</IU>
    <IU n="135">(0.5)</IU>
    <IU n="136">хаӈисьэ,</IU>
    <IU n="137">йод-ай-дэллэ,</IU>
    <IU n="138">(0.4)</IU>
    <IU n="139">тудэ нугэн-гэлэ ньанмэ шэльгэ-шэ-дэ-гэ,</IU>
    <IU n="140">(0.7)</IU>
    <IU n="141">чоҕойо-гэлэ кигии-м.</IU>
    <IU n="142">(1.2)</IU>
    <IU n="143">кэрэкэ-н хаӈисьэ-гэлэ,</IU>
    <IU n="144">(0.3)</IU>
    <IU n="145">[чоҕойо-дэ-йлэ] —</IU>
    <IU n="146">чоҕойо-дэ-йлэ йуукэ пэшшэй-м.</IU>
    <IU n="147">(1.2)</IU>
    <IU n="148">тамун-гэ кэрэкэ-н хаӈисьэ чирчэ-г-эй-дэ-гэ,</IU>
    <IU n="149">(0.8)</IU>
    <IU n="150">оду-н хаӈисьэ-гэлэ хожидэлбэ-гэн мин-дэ-гэ,</IU>
    <IU n="151">шубэжэ-ллэ,</IU>
    <IU n="152">(0.5)</IU>
    <IU n="153">тудэ чоҕойо-гэ таа-т мэ ам-дэ-й.</IU>
    <IU n="154">(1.8)</IU>
    <IU n="155">таа-т,</IU>
    <IU n="156">(0.9)</IU>
    <IU n="157">оду-н льэ,</IU>
    <IU n="158">(1.4)</IU>
    <IU n="159">хаӈисьэ нумө-ӈин киэ-сь,</IU>
    <IU n="160">(0.7)</IU>
    <IU n="161">кэл-дэ-гэ,</IU>
    <IU n="162">(0.4)</IU>
    <IU n="163">тудэ машльуө-ӈин мон-и,</IU>
    <IU n="164">(1.7)</IU>
    <IU n="165">хон-дэ-гэ мон мон-и,</IU>
    <IU n="166">тит эсьиэ эл ньиэ-ӈи-лэ-к,</IU>
    <IU n="167">тит эсьиэ эл кэл-у-т.</IU>
    <IU n="168">(1.9)</IU>
    <IU n="169">таа-т угуйэ-дэ-гэ эг-иэ-дэллэ,</IU>
    <IU n="170">(0.4)</IU>
    <IU n="171">оду-н,</IU>
    <IU n="172">(0.5)</IU>
    <IU n="173">хаӈисьэ,</IU>
    <IU n="174">(0.9)</IU>
    <IU n="175">[аасьэ-п-тэ-йлэ],</IU>
    <IU n="176">аасьэ-п-тэ-йлэ,</IU>
    <IU n="177">(0.4)</IU>
    <IU n="178">приагай-дэллэ,</IU>
    <IU n="179">лэгул-пэ-дэ-йлэ им-дэ-дэллэ,</IU>
    <IU n="180">(0.9)</IU>
    <IU n="181">дьэ,</IU>
    <IU n="182">(0.6)</IU>
    <IU n="183">чугиэ льэ-ӈи-к,</IU>
    <IU n="184">(0.3)</IU>
    <IU n="185">хон-тэ-ллэ,</IU>
    <IU n="186">(0.9)</IU>
    <IU n="187">титтэ лаӈи кэб-эй-ӈи-к мон-и.</IU>
    <IU n="188">(0.8)</IU>
    <IU n="189">идьии,</IU>
    <IU n="190">(0.7)</IU>
    <IU n="191">тит эсьиэ ам-дэ-й,</IU>
    <IU n="192">алб-оо-й,</IU>
    <IU n="193">(0.3)</IU>
    <IU n="194">мон-и идьии,</IU>
    <IU n="195">(0.4)</IU>
    <IU n="196">таа шоромо-ӈин,</IU>
    <IU n="197">(0.6)</IU>
    <IU n="198">кэрэкэ-пул-ӈин пунду-ӈи-к.</IU>
    <IU n="199">(1.4)</IU>
    <IU n="200">йах штоб эл кэл-ӈи-гэ-н ти-ӈидэ.</IU>
    <IU n="201">(0.7)</IU>
    <IU n="202">дьэ тамун йолаа-т эл кэл-у-ну-ӈи,</IU>
    <IU n="203">(0.7)</IU>
    <IU n="204">та-ӈ кэрэкэ-пул.</IU>
