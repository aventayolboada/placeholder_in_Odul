    <fileDesc>
      <title>The wonderful stick</title>
      <sourceDesc>author="Nikolaeva &amp; Mayer" yearpub="2004" location="https://www.sgr.fi/yukaghir/start1.html"</sourceDesc>
      <textDesc>variety="Kolyma Yukaghir" genre="Story" daterec="1990" locrec="Unknown"</textDesc>
      <particDesc>id="010" name="Vasilij Shalugin" age="56" gender="Male"</particDesc>
      <notes>The content of this text was recorded in Russian by Ljudmila Zhukova from one of the residents of the village Nelemnoe. In 1990 it was translated into Yukaghir by Vasilij Shalugin.</notes>
    </fileDesc>
  </teiHeader>
  <stext>
    <IU n="1">пооро-мэ,</IU>
    <IU n="2">(1.6)</IU>
    <IU n="3">эдуөн оо-дьоод-эк.</IU>
    <IU n="4">(3.0)</IU>
    <IU n="5">пөн,</IU>
    <IU n="6">(0.3)</IU>
    <IU n="7">пуг-у-лб-у-йэ-гэ.</IU>
    <IU n="8">(1.2)</IU>
    <IU n="9">мэт омо-сьэ,</IU>
    <IU n="10">(0.9)</IU>
    <IU n="11">пугэ-сьэ ньиэр моро-дэллэ,</IU>
    <IU n="12">(1.0)</IU>
    <IU n="13">мэт капкан-пэ йуө-дин кэб-э-сьэ.</IU>
    <IU n="14">(1.2)</IU>
    <IU n="15">[хам-у-н-дэ соболь-э ии-дэ-мэ],</IU>
    <IU n="16">(0.6)</IU>
    <IU n="17">таа-т эй-рэ-т,</IU>
    <IU n="18">(0.8)</IU>
    <IU n="19">хам-у-н-дэ нохшо ии-дэ-мэ,</IU>
    <IU n="20">(0.6)</IU>
    <IU n="21">йоло-ҕудэ ньаасьэ-д-ай-л-аа-йэ,</IU>
    <IU n="22">(1.7)</IU>
    <IU n="23">[унуӈ-гэн] —</IU>
    <IU n="24">(1.2)</IU>
    <IU n="25">унуӈ чаҕ-у-йии-т,</IU>
    <IU n="26">(2.3)</IU>
    <IU n="27">льэ-гэ,</IU>
    <IU n="28">(1.1)</IU>
    <IU n="29">альоойэ-гэ лөуд-ии-сьэ.</IU>
    <IU n="30">(0.9)</IU>
    <IU n="31">пуди лааҕ-э-т,</IU>
    <IU n="32">(1.5)</IU>
    <IU n="33">унуӈ йомул лааҕ-э-т,</IU>
    <IU n="34">(0.8)</IU>
    <IU n="35">пөн пуг-у-лбэ-дэ-йнэ,</IU>
    <IU n="36">(1.3)</IU>
    <IU n="37">йархэ аль-у-нун-ну-й.</IU>
    <IU n="38">(0.8)</IU>
    <IU n="39">льэ-льэ-дэллэ,</IU>
    <IU n="40">насиилэ,</IU>
    <IU n="41">(1.0)</IU>
    <IU n="42">[пукэльэ],</IU>
    <IU n="43">(0.5)</IU>
    <IU n="44">йархэ лөуд-уу-сьэ,</IU>
    <IU n="45">насиилэ пукэльэ-лэ,</IU>
    <IU n="46">(1.5)</IU>
    <IU n="47">льэ-гэ,</IU>
    <IU n="48">(2.2)</IU>
    <IU n="49">төб-оо-нун-ну-й.</IU>
    <IU n="50">(0.8)</IU>
    <IU n="51">чистэ йургуу тити-миэ-дьоон оо-нун-ну-й.</IU>
    <IU n="52">(0.6)</IU>
    <IU n="53">[оодо-лэ] —</IU>
    <IU n="54">оодо-лэк эрэ.</IU>
    <IU n="55">(1.1)</IU>
    <IU n="56">насиилэ оодо-н-ну-й.</IU>
    <IU n="57">(0.9)</IU>
    <IU n="58">йархэ аа-н мэт йаҕидьэ,</IU>
    <IU n="59">(0.3)</IU>
    <IU n="60">мэт аабэ хон-у-й,</IU>
    <IU n="61">(0.3)</IU>
    <IU n="62">льэ,</IU>
    <IU n="63">(0.6)</IU>
    <IU n="64">лөуд-уу-й.</IU>
    <IU n="65">(1.3)</IU>
    <IU n="66">мэт таа-т насиилэ арп-а-сьэ,</IU>
    <IU n="67">(0.9)</IU>
    <IU n="68">пукэльэ,</IU>
    <IU n="69">оҕун-дьэ,</IU>
    <IU n="70">(0.6)</IU>
    <IU n="71">угурчэ-гэ тон-д-уу-ги,</IU>
    <IU n="72">элльоодэй титэ тон-д-уу-й.</IU>
    <IU n="73">(2.0)</IU>
    <IU n="74">хон-у-л бойсьо,</IU>
    <IU n="75">нигэйэ-му-л хон-у-л.</IU>
    <IU n="76">(1.5)</IU>
    <IU n="77">мэт,</IU>
    <IU n="78">(1.5)</IU>
    <IU n="79">чугоо-н хон-дин таа-т гудэ-л-э-сьэ,</IU>
    <IU n="80">(0.9)</IU>
    <IU n="81">йаха-дин чугоо-н эл йарх-аа-дин.</IU>
    <IU n="82">(1.0)</IU>
    <IU n="83">пөн эми-дэ-сь,</IU>
    <IU n="84">(0.2)</IU>
    <IU n="85">эми-киэ-й.</IU>
    <IU n="86">(1.6)</IU>
    <IU n="87">[эми-киэ-й].</IU>
    <IU n="88">(1.9)</IU>
    <IU n="89">мэт палатка-ӈин,</IU>
    <IU n="90">(0.2)</IU>
    <IU n="91">ирк-и-н сьись-эк.</IU>
    <IU n="92">(0.6)</IU>
    <IU n="93">пөнь-оо-л.</IU>
    <IU n="94">(1.2)</IU>
    <IU n="95">[ирк-и-н],</IU>
    <IU n="96">(0.4)</IU>
    <IU n="97">ирк-и-н,</IU>
    <IU n="98">(0.6)</IU>
    <IU n="99">сьись-эк пөнь-оо-л,</IU>
    <IU n="100">(0.3)</IU>
    <IU n="101">таа ирк-и-н сьись,</IU>
    <IU n="102">(2.1)</IU>
    <IU n="103">та-ӈ сьись-эк ... лөуд-уу-ги надо,</IU>
    <IU n="104">(2.2)</IU>
    <IU n="105">[йахл-у-дэ өӈж-эй-дэллэ],</IU>
    <IU n="106">(4.5)</IU>
    <IU n="107">[йахл-у-дэ] —</IU>
    <IU n="108">йахл-у-дэ өӈж-эй-дэ-ӈидэ,</IU>
    <IU n="109">(4.5)</IU>
    <IU n="110">таӈдиэт мэт нумө-гэ йаха-тэ-йэ.</IU>
    <IU n="111">(1.2)</IU>
    <IU n="112">чандэ арп-у-йии-т,</IU>
    <IU n="113">насиилэ арп-у-й-дэ,</IU>
    <IU n="114">(0.4)</IU>
    <IU n="115">йоло-ҕудэ пол-лэ пөм-у-ду-йэ.</IU>
    <IU n="116">(1.2)</IU>
    <IU n="117">эл арп-ай-сьуөн.</IU>
    <IU n="118">(0.6)</IU>
    <IU n="119">таа-т хам-у-н-дэ ... таа-т льэ-й.</IU>
    <IU n="120">(0.9)</IU>
    <IU n="121">таа-т,</IU>
    <IU n="122">льэ йоло-ҕудэ пөм-у-дэ-сьэ.</IU>
    <IU n="123">пөм-у-дэ-сьэ.</IU>
    <IU n="124">(1.2)</IU>
    <IU n="125">мэт сьиилэ ул-у-му-й бойсьо.</IU>
    <IU n="126">(1.4)</IU>
    <IU n="127">мэт ужэ,</IU>
    <IU n="128">(1.4)</IU>
    <IU n="129">чуө,</IU>
    <IU n="130">(1.3)</IU>
    <IU n="131">мэт льэ,</IU>
    <IU n="132">(0.9)</IU>
    <IU n="133">мэт сьиилэ ул-у-му-й,</IU>
    <IU n="134">(0.5)</IU>
    <IU n="135">мэт палатка-гэ эл йаха-можии-йэ мон-у-т.</IU>
    <IU n="136">(0.9)</IU>
    <IU n="137">өнмэ-гэ,</IU>
    <IU n="138">(0.7)</IU>
    <IU n="139">мэт,</IU>
    <IU n="140">(0.3)</IU>
    <IU n="141">уөр-пэ,</IU>
    <IU n="142">(0.4)</IU>
    <IU n="143">мэт тэрикэ нумө-лэк.</IU>
    <IU n="144">(0.5)</IU>
    <IU n="145">өнмэ-гэ эй-уу-л,</IU>
    <IU n="146">(0.5)</IU>
    <IU n="147">таа мо-дьэ ноӈоон,</IU>
    <IU n="148">(0.2)</IU>
    <IU n="149">мэт тэрикэ-ньэ эр-и-сь,</IU>
    <IU n="150">ан-нь-аа-ну-льэл.</IU>
    <IU n="151">(0.6)</IU>
    <IU n="152">ноӈоон мэт —</IU>
    <IU n="153">мэт уөр-пэ-ньэ ньаҕаа эр-и-сь ан-нь-аа-ну-льэл.</IU>
    <IU n="154">(0.4)</IU>
    <IU n="155">мон-у-т өнмэ-гэ эй-тэ.</IU>
    <IU n="156">(0.6)</IU>
    <IU n="157">чумут аӈдьэ киэйэ эг-иэ-й,</IU>
    <IU n="158">(1.3)</IU>
    <IU n="159">бойсьо,</IU>
    <IU n="160">(2.5)</IU>
    <IU n="161">льэ-ӈоот,</IU>
    <IU n="162">(3.7)</IU>
    <IU n="163">бойсьо шөж-иэ-йэ,</IU>
    <IU n="164">(0.9)</IU>
    <IU n="165">ходо-т хам-у-н миинута-гэ ход-оо-йэ,</IU>
    <IU n="166">пукэльэ,</IU>
    <IU n="167">(0.4)</IU>
    <IU n="168">будиэ,</IU>
    <IU n="169">(0.8)</IU>
    <IU n="170">таа-т,</IU>
    <IU n="171">(0.7)</IU>
    <IU n="172">насиилэ,</IU>
    <IU n="173">(0.6)</IU>
    <IU n="174">эг-иэ-дэллэ,</IU>
    <IU n="175">(1.2)</IU>
    <IU n="176">арп-ай-дин,</IU>
    <IU n="177">(0.2)</IU>
    <IU n="178">кэб-эй-ль-иэ-йэ.</IU>
    <IU n="179">(0.7)</IU>
    <IU n="180">ай,</IU>
    <IU n="181">(0.5)</IU>
    <IU n="182">таа-т лөуд-ии-сьэ.</IU>
    <IU n="183">(1.4)</IU>
    <IU n="184">нугэн-э шаар-гэ-д эрэ,</IU>
    <IU n="185">(1.5)</IU>
    <IU n="186">арп-аа-йэ.</IU>
    <IU n="187">(1.4)</IU>
    <IU n="188">та-ӈ пиэ будиэ ирк-и-н ...,</IU>
    <IU n="189">(0.5)</IU>
    <IU n="190">шаал өйльэ-дэ-йнэ тит,</IU>
    <IU n="191">шаар-гэ-д эрэ эг-иэ-йэ.</IU>
    <IU n="192">(0.7)</IU>
    <IU n="193">мэт ной будиэ эг-иэ-йэ,</IU>
    <IU n="194">(1.1)</IU>
    <IU n="195">дьэ таа-т шубэж-иэ-йэ.</IU>
    <IU n="196">(1.6)</IU>
    <IU n="197">мэт бойсьо йоӈжо,</IU>
    <IU n="198">(0.6)</IU>
    <IU n="199">[шаар-гэ мэт] —</IU>
    <IU n="200">шаар-гэ эрэ,</IU>
    <IU n="201">(4.5)</IU>
    <IU n="202">эг-у-й-сьэ мон-у-т.</IU>
    <IU n="203">(6.8)</IU>
    <IU n="204">насиилэ,</IU>
    <IU n="205">аӈдьэ,</IU>
    <IU n="206">льэ-гэн,</IU>
    <IU n="207">(0.2)</IU>
    <IU n="208">йуө-тэ-й.</IU>
    <IU n="209">(1.6)</IU>
    <IU n="210">[йаҕидьэ] —</IU>
    <IU n="211">(0.4)</IU>
    <IU n="212">йаҕидьэ,</IU>
    <IU n="213">(0.9)</IU>
    <IU n="214">мэт йаҕидьэ.</IU>
    <IU n="215">(1.0)</IU>
    <IU n="216">йуө-лу-гэ,</IU>
    <IU n="217">(0.5)</IU>
    <IU n="218">... йаҕидьэ-к.</IU>
    <IU n="219">(1.0)</IU>
    <IU n="220">шэрил-оо-дьэ йаҕидьэ-к насиилэ.</IU>
    <IU n="221">(0.6)</IU>
    <IU n="222">аӈдьэ йуө-лу-гэ йуө.</IU>
    <IU n="223">(0.9)</IU>
    <IU n="224">[нумө-гэ йаха] —</IU>
    <IU n="225">палатка-гэ йаха-иили,</IU>
    <IU n="226">(0.7)</IU>
    <IU n="227">[мэт йаҕидьэ] —</IU>
    <IU n="228">та-ӈ йаҕидьэ,</IU>
    <IU n="229">(0.6)</IU>
    <IU n="230">палатка,</IU>
    <IU n="231">(0.7)</IU>
    <IU n="232">аӈиль-гэ таа льэ,</IU>
    <IU n="233">(0.5)</IU>
    <IU n="234">эгэ-тэ-т.</IU>
    <IU n="235">(1.9)</IU>
    <IU n="236">чоҕойо-лэ мэт льэ,</IU>
    <IU n="237">(1.6)</IU>
    <IU n="238">мэт ньиэр чуму чоу-ну-т.</IU>
    <IU n="239">(0.7)</IU>
    <IU n="240">алҕаар гудэ-йэ,</IU>
    <IU n="241">(0.8)</IU>
    <IU n="242">льэ-лэ,</IU>
    <IU n="243">(1.8)</IU>
    <IU n="244">одэколон-э мэ=т чаҕ-и-тэ-йэ,</IU>
    <IU n="245">(0.9)</IU>
    <IU n="246">илльэ ньиэр моро-дэллэ,</IU>
    <IU n="247">(1.4)</IU>
    <IU n="248">мэт оҕох-эк пиэдэ-т-мэ.</IU>
    <IU n="249">(0.7)</IU>
    <IU n="250">чай оожэ-ллэ,</IU>
    <IU n="251">(0.3)</IU>
    <IU n="252">таӈдиэт уӈж-и-сьэ.</IU>
    <IU n="253">(0.3)</IU>
    <IU n="254">эльльоой—</IU>
    <IU n="255">(0.3)</IU>
    <IU n="256">тоҕоо-дьэ уӈж-оо-ну-льэл-дьэ.</IU>
    <IU n="257">(0.8)</IU>
    <IU n="258">мөж-ж-эй-дэллэ,</IU>
    <IU n="259">(0.6)</IU>
    <IU n="260">мэт оҕох пиэдэ-т-мэ,</IU>
    <IU n="261">(0.6)</IU>
    <IU n="262">таа-т,</IU>
    <IU n="263">(0.5)</IU>
    <IU n="264">льэ-йэ,</IU>
    <IU n="265">(1.3)</IU>
    <IU n="266">миэ-дэ-йэ ханьин,</IU>
    <IU n="267">(0.5)</IU>
    <IU n="268">пөн подьорхо-т мон-у-т.</IU>
    <IU n="269">(1.9)</IU>
    <IU n="270">угуйэ-л-мэ подьорхо-т-тэ-йнэ,</IU>
    <IU n="271">(0.7)</IU>
    <IU n="272">нумун йаҕидьэ-к.</IU>
    <IU n="273">(0.7)</IU>
    <IU n="274">мэт тамун-гэ,</IU>
    <IU n="275">(0.7)</IU>
    <IU n="276">эл ду,</IU>
    <IU n="277">(0.6)</IU>
    <IU n="278">тамун йуө-дин.</IU>
    <IU n="279">(1.2)</IU>
    <IU n="280">палатка-гэт ук-эй-дэллэ,</IU>
    <IU n="281">(1.3)</IU>
    <IU n="282">йуө-ну —</IU>
    <IU n="283">(0.5)</IU>
    <IU n="284">аӈсьии-д-аа-т йуө-ну-йэ.</IU>
    <IU n="285">(0.5)</IU>
    <IU n="286">та-ӈ шаал ньэ хадунгэ өйльэ.</IU>
    <IU n="287">(1.2)</IU>
    <IU n="288">та-ӈ мэт,</IU>
    <IU n="289">(0.5)</IU>
    <IU n="290">йаҕидьэ оо-жии-т кэсьии-мэ шаал,</IU>
    <IU n="291">(1.2)</IU>
    <IU n="292">чугө-ги нутньэ-й,</IU>
    <IU n="293">(0.6)</IU>
    <IU n="294">а,</IU>
    <IU n="295">шаал-ги өйльэ.</IU>
    <IU n="296">(0.4)</IU>
    <IU n="297">палатка-гэ,</IU>
    <IU n="298">(1.0)</IU>
    <IU n="299">йөмг-ии-т эй-рэ-йэ,</IU>
    <IU n="300">(0.3)</IU>
    <IU n="301">ньэ лэмэ-лэ эл нуу.</IU>
    <IU n="302">(1.2)</IU>
    <IU n="303">өнмэ эй-рэ-ш-у-т мо-дьэ,</IU>
    <IU n="304">(0.7)</IU>
    <IU n="305">лэбиэ-н будиэ,</IU>
    <IU n="306">(0.7)</IU>
    <IU n="307">шаар эрэ,</IU>
    <IU n="308">(1.0)</IU>
    <IU n="309">шоромо эл лэй-дии-йоон льэ-й.</IU>
  </stext>
