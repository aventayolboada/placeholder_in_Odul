  <stext>
    <IU n="1">пулут тэрикэ-ньэ модо-льэл-ӈи,</IU>
    <IU n="2">ирк-и-н,</IU>
    <IU n="3">(1.1)</IU>
    <IU n="4">адуө-ньэ-льэл-ӈи.</IU>
    <IU n="5">адуө-пэ-ги модо-т мон-и,</IU>
    <IU n="6">мэт тэрикэ-т-тэ-йэ мон-и.</IU>
    <IU n="7">эди-ӈ пулут мон-и,</IU>
    <IU n="8">хон,</IU>
    <IU n="9">кэб-эй-к,</IU>
    <IU n="10">тэт тэрикэ-можуу аӈсьии-йии-к.</IU>
    <IU n="11">маҕил-гэлэ моро-м,</IU>
    <IU n="12">(0.6)</IU>
    <IU n="13">моро-дэллэ кэб-э-с.</IU>
    <IU n="14">хон-дэ-гэ,</IU>
    <IU n="15">(0.2)</IU>
    <IU n="16">иӈэр оожии-гэ,</IU>
    <IU n="17">ирк-и-н анил,</IU>
    <IU n="18">(1.0)</IU>
    <IU n="19">анил-диэ-к ход-оо-л.</IU>
    <IU n="20">тамун-гэлэ тудэ маҕил лөудэ-ллэ,</IU>
    <IU n="21">(0.8)</IU>
    <IU n="22">тудэ маҕил-э хаӈииноо-рэ-ллэ,</IU>
    <IU n="23">(0.3)</IU>
    <IU n="24">хон-дэллэ унуӈ-ӈин,</IU>
    <IU n="25">пэшшэй-м.</IU>
    <IU n="26">таа-т,</IU>
    <IU n="27">(0.2)</IU>
    <IU n="28">кэб-эй-льэл.</IU>
    <IU n="29">хон-и —</IU>
    <IU n="30">хон-у-т,</IU>
    <IU n="31">(2.0)</IU>
    <IU n="32">омнии-гэ йаха-й.</IU>
    <IU n="33">омнии-гэ йаха-дэллэ,</IU>
    <IU n="34">(0.2)</IU>
    <IU n="35">ирк-и-н пулут-эк льэ-льэл-у-л.</IU>
    <IU n="36">(0.9)</IU>
    <IU n="37">машльуө-ньэ-й,</IU>
    <IU n="38">көрөлькии-лэ мин-дэллэ оожии-ӈин пэшшэй-м,</IU>
    <IU n="39">пэшшэй-дэллэ мон-и кин көрөлькии-гэт чистэ чумут шахальэ-ш-у-т,</IU>
    <IU n="40">(0.7)</IU>
    <IU n="41">тамуӈ-ӈин мэт мархиль оо-тэ-й.</IU>
    <IU n="42">кэл-у-л көй унуӈ-ӈин өӈж-э-сь,</IU>
    <IU n="43">(0.5)</IU>
    <IU n="44">унуӈ-гэ оҕ-оо-дэ-гэ,</IU>
    <IU n="45">(0.7)</IU>
    <IU n="46">хон-дэллэ оҕ-оо-дэллэ мон-и.</IU>
    <IU n="47">(2.0)</IU>
    <IU n="48">тии-нэ мэт оожии-ӈин пэшшэй-мэ анил,</IU>
    <IU n="49">(1.0)</IU>
    <IU n="50">мит көрөлькии,</IU>
    <IU n="51">(0.6)</IU>
    <IU n="52">шахальэ-ш-тэллэ кэсьии-к.</IU>
    <IU n="53">таа-т оҕ-оо-дэ-гэ анил,</IU>
    <IU n="54">(0.5)</IU>
    <IU n="55">киэ-сь,</IU>
    <IU n="56">(0.7)</IU>
    <IU n="57">тудэ аӈа-гэ мой-м,</IU>
    <IU n="58">(0.2)</IU>
    <IU n="59">көрөлькии-лэ.</IU>
    <IU n="60">көй мидь-у-м,</IU>
    <IU n="61">мин-дэллэ йообии арп-а-сь,</IU>
    <IU n="62">арп-ай-дэллэ та-ӈ пулут-ӈин тадии-м.</IU>
    <IU n="63">та-ӈ пулут,</IU>
    <IU n="64">(0.2)</IU>
    <IU n="65">көрөлькии мин-дэллэ чуӈ-у-м,</IU>
    <IU n="66">чумут льэ-й,</IU>
    <IU n="67">таӈнуги мон-и,</IU>
    <IU n="68">тудэ,</IU>
    <IU n="69">(0.4)</IU>
    <IU n="70">мархи-д уө-ӈин.</IU>
    <IU n="71">(1.0)</IU>
    <IU n="72">ти-ӈ көй-ӈин,</IU>
    <IU n="73">(0.4)</IU>
    <IU n="74">тэрикэ-д-оон хон.</IU>
    <IU n="75">таа,</IU>
    <IU n="76">(0.8)</IU>
    <IU n="77">тиӈта-ӈ көй тудэ тэрикэ-ньэ модо-ӈи,</IU>
    <IU n="78">эл иись —</IU>
    <IU n="79">ньэ иись модо-ӈи-дэ-гэ,</IU>
    <IU n="80">(2.2)</IU>
    <IU n="81">пулут мон-и,</IU>
    <IU n="82">(0.9)</IU>
    <IU n="83">эл иись иись льэ-можии-мэт,</IU>
    <IU n="84">тит лэбиэ-ӈин кэб-эй-ӈи-к.</IU>
    <IU n="85">пулут,</IU>
    <IU n="86">(2.0)</IU>
    <IU n="87">мон-оо-дэ йолаа-н,</IU>
    <IU n="88">(0.9)</IU>
    <IU n="89">көй тэрикэ-дэ-ньэ кэб-эй-ӈи,</IU>
    <IU n="90">титтэ лэбиэ-ӈин,</IU>
    <IU n="91">титтэ лэбиэ-гэ йаха-ӈи.</IU>
    <IU n="92">көй нумө-ги айии оҕ-оо-й,</IU>
    <IU n="93">ти-ӈ параа лаӈи модо-льэл-ӈи.</IU>
    <IU n="94">...</IU>
