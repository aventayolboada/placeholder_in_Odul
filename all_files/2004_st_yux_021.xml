  <stext>
    <IU n="1">эрпэйэ-пул молҕо-до-ҕо,</IU>
    <IU n="2">(0.6)</IU>
    <IU n="3">ирк-и-н,</IU>
    <IU n="4">(0.2)</IU>
    <IU n="5">улум гуду-й-сьоод-эк,</IU>
    <IU n="6">(0.2)</IU>
    <IU n="7">льэ-льэл-у-л,</IU>
    <IU n="8">йэльизар мон-у-т ньиэ-ньэ-ӈаа.</IU>
    <IU n="9">(0.6)</IU>
    <IU n="10">тудэ,</IU>
    <IU n="11">(0.7)</IU>
    <IU n="12">шоромо-пул-гэлэ чуму куд-сьии-льэл-у-м,</IU>
    <IU n="13">(0.8)</IU>
    <IU n="14">аасьэ-пул ай,</IU>
    <IU n="15">(0.3)</IU>
    <IU n="16">йиэн шоромо-пул кэл-ӈи-дэ-гэ,</IU>
    <IU n="17">йуө-ӈи-дэ-гэ,</IU>
    <IU n="18">(0.4)</IU>
    <IU n="19">тудэ нумө хонжо-ш-тэллэ эй-рэ-т эг-у-жу-льэл.</IU>
    <IU n="20">(1.2)</IU>
    <IU n="21">тудэ идьиэ,</IU>
    <IU n="22">(0.3)</IU>
    <IU n="23">йообии.</IU>
    <IU n="24">(1.4)</IU>
    <IU n="25">мэ эй-рэ-й.</IU>
    <IU n="26">(0.4)</IU>
    <IU n="27">тудэ-гэлэ,</IU>
    <IU n="28">(0.7)</IU>
    <IU n="29">мин-дин льэ-льэл-ӈаа,</IU>
    <IU n="30">(0.9)</IU>
    <IU n="31">мил-лэ лор-х-ай-льэл-ӈаа.</IU>
    <IU n="32">эл мин-дьии-сьуөн льэ-ну-льэл,</IU>
    <IU n="33">(0.5)</IU>
    <IU n="34">шэй-р-эй-льэл таа.</IU>
    <IU n="35">(0.2)</IU>
    <IU n="36">шэй-р-эй-л-оо-дэ йолаа-т,</IU>
    <IU n="37">(0.3)</IU>
    <IU n="38">аӈсьии-дэллэ,</IU>
    <IU n="39">аӈсьии-дэллэ таа эл нун-ну-льэл-ӈи.</IU>
    <IU n="40">(0.6)</IU>
    <IU n="41">атахл-э-ш-тэ ньэмолҕил-гэ арҕаатаас-кэ йуө-ӈи-дэ-гэ,</IU>
    <IU n="42">(1.4)</IU>
    <IU n="43">ирк-и-н шоромо-к.</IU>
    <IU n="44">(1.2)</IU>
    <IU n="45">кэсьии-льэл-ӈаа,</IU>
    <IU n="46">(0.4)</IU>
    <IU n="47">шаал-гэ ултэ-льэл-ну-льэл-ӈаа,</IU>
    <IU n="48">(0.5)</IU>
    <IU n="49">тудэ-л ам-дэ-й.</IU>
    <IU n="50">(0.6)</IU>
    <IU n="51">йашик молҕо шиншай-льэл-ӈаа.</IU>
    <IU n="52">(0.3)</IU>
    <IU n="53">атахл-э-ш-тэ подьорхо-гэ,</IU>
    <IU n="54">(0.8)</IU>
    <IU n="55">ньугэрэ доска-ги,</IU>
    <IU n="56">(2.2)</IU>
    <IU n="57">мэрэ-й-льэл йиэд-у-д.</IU>
    <IU n="58">(2.0)</IU>
    <IU n="59">ирк-и-н кукушка пуй-л тити-миэ-дьоод-э чом-му-льэл.</IU>
    <IU n="60">атахл-э-ш-тэ,</IU>
    <IU n="61">(0.4)</IU>
    <IU n="62">эмил-гэ,</IU>
    <IU n="63">(0.6)</IU>
    <IU n="64">ай та-ӈ доска-пэ-ги хонжо-ш-льэл-у-м.</IU>
    <IU n="65">(0.5)</IU>
    <IU n="66">хонжо-льэл-ӈи.</IU>
    <IU n="67">(0.3)</IU>
    <IU n="68">ай ирк-и-н кукушка пуй-л тити-миэ-дьоод-э,</IU>
    <IU n="69">(0.5)</IU>
    <IU n="70">чом-му-льэл.</IU>
    <IU n="71">(0.9)</IU>
    <IU n="72">таӈдиэт,</IU>
    <IU n="73">та-ӈ,</IU>
    <IU n="74">йашик-кэлэ пэшшэй-льэл-ӈаа,</IU>
    <IU n="75">(0.2)</IU>
    <IU n="76">[миэтрэ эймундэ тити-миэ-дьоод-э чомо] —</IU>
    <IU n="77">[чомо],</IU>
    <IU n="78">миэтрэ,</IU>
    <IU n="79">(0.2)</IU>
    <IU n="80">эймундэ тити-миэ-дьоод-э чом-оо-жии-т аа-льэл-ӈаа.</IU>
    <IU n="81">(1.0)</IU>
    <IU n="82">йообии,</IU>
    <IU n="83">(0.4)</IU>
    <IU n="84">хон-тэллэ,</IU>
    <IU n="85">(1.2)</IU>
    <IU n="86">тох-тэллэ пэшшэй-льэл-ӈаа.</IU>
    <IU n="87">(1.4)</IU>
    <IU n="88">пудэ-гэ,</IU>
    <IU n="89">(3.6)</IU>
    <IU n="90">[лабаз],</IU>
    <IU n="91">(0.7)</IU>
    <IU n="92">иэ,</IU>
    <IU n="93">льэ-лэк,</IU>
    <IU n="94">(0.5)</IU>
    <IU n="95">ньибойэ-лэк аа-льэл-ӈилэ.</IU>
    <IU n="96">[эй-оо-тэ-й-бэн-гэ],</IU>
    <IU n="97">(1.8)</IU>
    <IU n="98">иэ.</IU>
    <IU n="99">тамун льэ-гэ,</IU>
    <IU n="100">(0.8)</IU>
    <IU n="101">эй-оо-тэ-й-бэн-дэ-гэ кэл-льэл-ӈи,</IU>
    <IU n="102">(0.4)</IU>
    <IU n="103">та-ӈ,</IU>
    <IU n="104">(1.0)</IU>
    <IU n="105">ньибойэ-п-ки дэль- —</IU>
    <IU n="106">дэльбэ чит-и-г-эй-льэл.</IU>
    <IU n="107">(1.2)</IU>
    <IU n="108">тамун-гэлэ,</IU>
    <IU n="109">(1.0)</IU>
    <IU n="110">таа-т иӈэр-тиэ-льэл-ӈаа.</IU>
    <IU n="111">(0.5)</IU>
    <IU n="112">йуө-ӈи-дэ-гэ,</IU>
    <IU n="113">(0.2)</IU>
    <IU n="114">та-ӈ шоромо,</IU>
    <IU n="115">(1.5)</IU>
    <IU n="116">сьись-кэ,</IU>
    <IU n="117">сьись-кэ шоромо-ӈоот кудэ-льэл.</IU>
    <IU n="118">(0.8)</IU>
    <IU n="119">өнмэ эй-рэ-ш-у-т,</IU>
    <IU n="120">(2.0)</IU>
    <IU n="121">мол-льэл-ӈи,</IU>
    <IU n="122">(0.3)</IU>
    <IU n="123">ходит таа-т гудэ-т.</IU>
    <IU n="124">(0.4)</IU>
    <IU n="125">пулут-пэ моӈ-ӈи,</IU>
    <IU n="126">(0.5)</IU>
    <IU n="127">лигэ-йоон-пэ-к,</IU>
    <IU n="128">(0.4)</IU>
    <IU n="129">та-ӈидэ эл эй-рэ-ӈи-лэ-к.</IU>
  </stext>
