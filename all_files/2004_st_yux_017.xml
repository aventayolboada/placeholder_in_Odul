  <stext>
    <IU n="1">тиотиа ксьэньийа.</IU>
    <IU n="2">(1.2)</IU>
    <IU n="3">тудаа,</IU>
    <IU n="4">(1.1)</IU>
    <IU n="5">ньиидьии-ну-льэл.</IU>
    <IU n="6">(0.8)</IU>
    <IU n="7">тудаа мит уө-ӈоо-т,</IU>
    <IU n="8">(0.9)</IU>
    <IU n="9">унуӈ-гэ,</IU>
    <IU n="10">(1.4)</IU>
    <IU n="11">абуд-аа-дэллэ.</IU>
    <IU n="12">(1.7)</IU>
    <IU n="13">табуд-э,</IU>
    <IU n="14">(2.7)</IU>
    <IU n="15">пукэльэ-гэ абуд-аа-дэллэ.</IU>
    <IU n="16">та-ӈ мит.</IU>
    <IU n="17">(0.6)</IU>
    <IU n="18">ход-оо-л-оо-л-оо.</IU>
    <IU n="19">(0.8)</IU>
    <IU n="20">табуд-э ворожии-нун-ну-й.</IU>
    <IU n="21">мит.</IU>
    <IU n="22">(0.8)</IU>
    <IU n="23">киэйэ ...</IU>
    <IU n="24">(0.6)</IU>
    <IU n="25">абуд-аа-дэллэ,</IU>
    <IU n="26">(2.9)</IU>
    <IU n="27">эг-иэ-ги надо.</IU>
    <IU n="28">(0.5)</IU>
    <IU n="29">што,</IU>
    <IU n="30">(1.0)</IU>
    <IU n="31">эл льэ-гэ-н.</IU>
    <IU n="32">(0.6)</IU>
    <IU n="33">эл хонжо-сьуөн,</IU>
    <IU n="34">ходо мит ход-оо-л-оок,</IU>
    <IU n="35">(0.3)</IU>
    <IU n="36">пукэльэ эл эй-тэ-сьуөн.</IU>
    <IU n="37">(0.4)</IU>
    <IU n="38">эг-иэ-дэллэ.</IU>
    <IU n="39">(1.6)</IU>
    <IU n="40">эл топтай-дин.</IU>
    <IU n="41">(1.1)</IU>
    <IU n="42">эл эг-иэ-рэ-сьуөн.</IU>
    <IU n="43">(0.4)</IU>
    <IU n="44">эг-иэ-дэллэ кэб-эй-дин.</IU>
    <IU n="45">(1.3)</IU>
    <IU n="46">таа,</IU>
    <IU n="47">(1.0)</IU>
    <IU n="48">угуйэ-ш-тэ кэл-дэллэ йуө-нун-ну-й.</IU>
    <IU n="49">(0.9)</IU>
    <IU n="50">нэмэ-н,</IU>
    <IU n="51">(1.0)</IU>
    <IU n="52">чугө-к льэ-л.</IU>
    <IU n="53">(1.5)</IU>
    <IU n="54">тоуко чугө льэ-дэ-йнэ.</IU>
    <IU n="55">(1.6)</IU>
    <IU n="56">то,</IU>
    <IU n="57">(0.3)</IU>
    <IU n="58">льэ-ӈин [одул-ӈин],</IU>
    <IU n="59">(2.1)</IU>
    <IU n="60">одул-ӈин хон-тэ-йэк.</IU>
    <IU n="61">(0.9)</IU>
    <IU n="62">йэсльи хоробо-н,</IU>
    <IU n="63">(0.8)</IU>
    <IU n="64">хохсьин ду йахадаасьэ-н хохсьин льэ-дэ-йнэ,</IU>
    <IU n="65">(1.7)</IU>
    <IU n="66">таӈдиэт йахал-ӈин хон-тэ-йэк.</IU>
    <IU n="67">(0.7)</IU>
    <IU n="68">йэсльи,</IU>
    <IU n="69">(0.8)</IU>
    <IU n="70">аасьэ-н.</IU>
    <IU n="71">(1.0)</IU>
    <IU n="72">чугө оо-дэ-йнэ,</IU>
    <IU n="73">(2.9)</IU>
    <IU n="74">эрпэйэ-ӈин.</IU>
    <IU n="75">(0.8)</IU>
    <IU n="76">тэрикэ.</IU>
    <IU n="77">(2.0)</IU>
    <IU n="78">оо-тэ-йэк.</IU>
  </stext>
