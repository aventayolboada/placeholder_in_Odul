    <IU n="4">лэбиэ-гэ,</IU>
    <IU n="5">льэ-й,</IU>
    <IU n="6">ксозиаин-ги.</IU>
    <IU n="7">(0.9)</IU>
    <IU n="8">хозиаин-гэт,</IU>
    <IU n="9">(0.3)</IU>
    <IU n="10">шоромо,</IU>
    <IU n="11">(0.3)</IU>
    <IU n="12">лэбиэ-н хозиаин-гэт шоромо ньиэ-нун-дьии-дэ-йнэ,</IU>
    <IU n="13">(0.8)</IU>
    <IU n="14">мусьин-бэн-гэлэ,</IU>
    <IU n="15">чуму кэй-ну-м,</IU>
    <IU n="16">чомоолбэд-эк кэй-ну-млэ,</IU>
    <IU n="17">миэмиэ-к кэй-ну-млэ,</IU>
    <IU n="18">(0.8)</IU>
    <IU n="19">льэ-лэк,</IU>
    <IU n="20">(0.4)</IU>
    <IU n="21">лиса-к кэй-ну-млэ,</IU>
    <IU n="22">соболь-эк кэй-ну-млэ,</IU>
    <IU n="23">горностай-к кэй-ну-млэ,</IU>
    <IU n="24">йододьубэ-лэк кэй-ну-млэ,</IU>
    <IU n="25">(0.3)</IU>
    <IU n="26">мусьин-бэн-гэлэ чуму кэй-ну-м.</IU>
    <IU n="27">(1.1)</IU>
    <IU n="28">лосьил-ӈин,</IU>
    <IU n="29">льэ пэйжии-ну-мэк.</IU>
    <IU n="30">лэгул пэйжии-ну-мэк.</IU>
    <IU n="31">(0.7)</IU>
    <IU n="32">таӈньиги чумут,</IU>
    <IU n="33">кэй-ну-млэ.</IU>
    <IU n="34">(0.3)</IU>
    <IU n="35">таасилэ,</IU>
    <IU n="36">йоӈж-оо-дьэ-ньут даҕаны,</IU>
    <IU n="37">... йуө-ну-мэк.</IU>
    <IU n="38">(0.7)</IU>
    <IU n="39">зиэмльа хозиаин,</IU>
    <IU n="40">(0.6)</IU>
    <IU n="41">йуө-нун-ну-й.</IU>
    <IU n="42">(1.0)</IU>
    <IU n="43">вот,</IU>
    <IU n="44">(0.4)</IU>
    <IU n="45">таасилэ,</IU>
    <IU n="46">(0.5)</IU>
    <IU n="47">омо-сьэ охотньик-пэ-ӈин даҕаны,</IU>
    <IU n="48">(0.8)</IU>
    <IU n="49">льэ-ну-й —</IU>
    <IU n="50">йэд-эй-ну-льэл.</IU>
    <IU n="51">(1.2)</IU>
    <IU n="52">ханьин эрэ,</IU>
    <IU n="53">(0.5)</IU>
    <IU n="54">ирк-и-н шоромо-ӈин тудаа йэд-эй-льэл,</IU>
    <IU n="55">(0.6)</IU>
    <IU n="56">зиэмльа хозиаин.</IU>
    <IU n="57">(0.5)</IU>
    <IU n="58">тамун,</IU>
    <IU n="59">(0.4)</IU>
    <IU n="60">йэд-эй-дэллэ,</IU>
    <IU n="61">(0.7)</IU>
    <IU n="62">мол-льэл,</IU>
    <IU n="63">(0.6)</IU>
    <IU n="64">мэт-и-н,</IU>
    <IU n="65">(0.5)</IU>
    <IU n="66">тэт,</IU>
    <IU n="67">(0.3)</IU>
    <IU n="68">льэ-к.</IU>
    <IU n="69">(2.2)</IU>
    <IU n="70">испиир кэсьии-к,</IU>
    <IU n="71">(1.0)</IU>
    <IU n="72">ирк-и-н,</IU>
    <IU n="73">бочка испиир,</IU>
    <IU n="74">(0.4)</IU>
    <IU n="75">кэсьии-к.</IU>
    <IU n="76">(0.7)</IU>
    <IU n="77">пльага,</IU>
    <IU n="78">(0.9)</IU>
    <IU n="79">кэсьии-к.</IU>
    <IU n="80">(0.6)</IU>
    <IU n="81">тамун таа-т кэсьии-лу-кэнэ,</IU>
    <IU n="82">(0.7)</IU>
    <IU n="83">мэт тэт-и-н льэ-лэк,</IU>
    <IU n="84">пушньина-лэк кэй-тэ-м.</IU>
    <IU n="85">(1.0)</IU>
    <IU n="86">йаа-н,</IU>
    <IU n="87">(0.7)</IU>
    <IU n="88">йаа-н куру-ньэ-й.</IU>
    <IU n="89">(0.5)</IU>
    <IU n="90">та-ӈ курул-ги,</IU>
    <IU n="91">(0.7)</IU>
    <IU n="92">букатын полный,</IU>
    <IU n="93">(0.7)</IU>
    <IU n="94">пушньина-лэк.</IU>
    <IU n="95">шахалэ-лэк,</IU>
    <IU n="96">горнатаа,</IU>
    <IU n="97">(0.8)</IU>
    <IU n="98">разный,</IU>
    <IU n="99">чумут льэ-й.</IU>
    <IU n="100">(1.4)</IU>
    <IU n="101">миэмиэ-н хаар,</IU>
    <IU n="102">көдиэ-н хаар,</IU>
    <IU n="103">чистэ чуму.</IU>
    <IU n="104">(0.8)</IU>
    <IU n="105">дьэ та-ӈ шоромо,</IU>
    <IU n="106">льэ-лэк,</IU>
    <IU n="107">...</IU>
    <IU n="108">(1.0)</IU>
    <IU n="109">купиэц-эк,</IU>
    <IU n="110">та-ӈ купэч-ӈин хон-дэ-гэ дьэ ирк-и-н пльага-гэлэ,</IU>
    <IU n="111">(0.5)</IU>
    <IU n="112">ирк-и-н,</IU>
    <IU n="113">льэ-гэлэ,</IU>
    <IU n="114">бочка-гэлэ мидь-у-м.</IU>
    <IU n="115">(0.6)</IU>
    <IU n="116">мин-дэллэ —</IU>
    <IU n="117">(0.6)</IU>
    <IU n="118">мин-дэллэ хон-тэ-м,</IU>
    <IU n="119">хон-тэ-ллэ,</IU>
    <IU n="120">(1.2)</IU>
    <IU n="121">тадии-м,</IU>
    <IU n="122">зиэмльа хозиаин-ӈин.</IU>
    <IU n="123">(1.5)</IU>
    <IU n="124">атахл —</IU>
    <IU n="125">йаалм-и-дьэ льэ-гэ,</IU>
    <IU n="126">кружка-гэ абут-тэ-м,</IU>
    <IU n="127">оожэ-м,</IU>
    <IU n="128">(0.5)</IU>
    <IU n="129">бочка ньэту.</IU>
    <IU n="130">(1.5)</IU>
    <IU n="131">всьо выпил.</IU>
    <IU n="132">(0.4)</IU>
    <IU n="133">вот.</IU>
    <IU n="134">(0.9)</IU>
    <IU n="135">дьэ мон-и,</IU>
    <IU n="136">(1.8)</IU>
    <IU n="137">лэм-дик анурэ-мэ,</IU>
    <IU n="138">(0.4)</IU>
    <IU n="139">чуму мин.</IU>
    <IU n="140">(0.5)</IU>
    <IU n="141">адуөн курул-гэт.</IU>
    <IU n="142">(1.1)</IU>
    <IU n="143">ходи-миэ-й шахалэ-к,</IU>
    <IU n="144">чэрнобурка-к надо-ӈоо-л ду,</IU>
    <IU n="145">красный льиса-к надо-ӈоо-л ду,</IU>
    <IU n="146">(0.5)</IU>
    <IU n="147">йододьубэ-лэк надо-ӈоо-л ду,</IU>
    <IU n="148">чуму мин.</IU>
    <IU n="149">(1.0)</IU>
    <IU n="150">сколько тиэбиэ,</IU>
    <IU n="151">хочэтсьа.</IU>
    <IU n="152">(0.9)</IU>
    <IU n="153">дьэ —</IU>
    <IU n="154">та-ӈ шоромо,</IU>
    <IU n="155">миидьии-гэ,</IU>
    <IU n="156">(0.4)</IU>
    <IU n="157">таа-т,</IU>
    <IU n="158">... им-дэ-м.</IU>
    <IU n="159">(0.9)</IU>
    <IU n="160">полный.</IU>
    <IU n="161">(1.0)</IU>
    <IU n="162">им-дэ-гэ нумө-ӈин кэб-э-сь.</IU>
    <IU n="163">(0.5)</IU>
    <IU n="164">нумө-ӈин кэб-эй-дэллэ,</IU>
    <IU n="165">(1.4)</IU>
    <IU n="166">кэл-гэ-к мон-и ай.</IU>
    <IU n="167">(1.0)</IU>
    <IU n="168">ай мон-и,</IU>
    <IU n="169">льэ,</IU>
    <IU n="170">миноо,</IU>
    <IU n="171">кэсьии-гэ-к.</IU>
    <IU n="172">(0.5)</IU>
    <IU n="173">аҕа,</IU>
    <IU n="174">кэсьи.</IU>
    <IU n="175">(0.9)</IU>
    <IU n="176">дьэ,</IU>
    <IU n="177">ай льэ-й,</IU>
    <IU n="178">(0.4)</IU>
    <IU n="179">ай кэб-э-сь.</IU>
    <IU n="180">кэб-эй-дэллэ,</IU>
    <IU n="181">(0.6)</IU>
    <IU n="182">как раз йуө-м,</IU>
    <IU n="183">кэб-эй-дэ-гэ кэл-дэ параа-гэ нумө-ӈин,</IU>
    <IU n="184">кэл-дэ параа-гэ,</IU>
    <IU n="185">(0.2)</IU>
    <IU n="186">пурга-ӈоот кудэ-й.</IU>
    <IU n="187">(0.7)</IU>
    <IU n="188">таа-т кэб-эй-дэ параа-гэ,</IU>
    <IU n="189">опиат,</IU>
    <IU n="190">пурга-ӈоот кудэ-й.</IU>
    <IU n="191">(1.3)</IU>
    <IU n="192">илэйэ-ӈоот кудэ-й,</IU>
    <IU n="193">пукэльэ йиэ-сь.</IU>
    <IU n="194">(0.8)</IU>
    <IU n="195">дьэ тудэ бочка-гэлэ ай көуд-эй-м.</IU>
    <IU n="196">(1.0)</IU>
    <IU n="197">дьэ,</IU>
    <IU n="198">тадии-м,</IU>
    <IU n="199">[тадии-дэ-гэ] —</IU>
    <IU n="200">тадии-дэ-гэ,</IU>
    <IU n="201">ай мон-и,</IU>
    <IU n="202">атахл-э-ш-тэ-гэ.</IU>
    <IU n="203">(0.7)</IU>
    <IU n="204">дьэ мон-и,</IU>
    <IU n="205">пушньина мин мон-и.</IU>
    <IU n="206">(0.7)</IU>
    <IU n="207">амбар-гэ.</IU>
    <IU n="208">(0.6)</IU>
    <IU n="209">йахл-оо-л амбар,</IU>
    <IU n="210">йоҕо-дай-к мон-и.</IU>
    <IU n="211">(0.7)</IU>
    <IU n="212">таа,</IU>
    <IU n="213">полно мон-и,</IU>
    <IU n="214">там,</IU>
    <IU n="215">таа-т.</IU>
    <IU n="216">(0.5)</IU>
    <IU n="217">мин мон-и.</IU>
    <IU n="218">(0.3)</IU>
    <IU n="219">мин-дэллэ нумө-ӈин көуд-эй-к мон-и.</IU>
    <IU n="220">(0.8)</IU>
    <IU n="221">только мон-и,</IU>
    <IU n="222">ньикому —</IU>
    <IU n="223">ньикому нил-ӈин эл мол-лэ-к мон-и,</IU>
    <IU n="224">ньикому ньэ —</IU>
    <IU n="225">ньикому ньэ говории сказал.</IU>
    <IU n="226">вот,</IU>
    <IU n="227">(2.2)</IU>
    <IU n="228">это,</IU>
    <IU n="229">туөн,</IU>
    <IU n="230">(0.8)</IU>
    <IU n="231">тамун-гэн дьэ,</IU>
    <IU n="232">мон-и,</IU>
    <IU n="233">(1.2)</IU>
    <IU n="234">тэт шоромо-ӈин эл пунду-лэ-к,</IU>
    <IU n="235">(1.4)</IU>
    <IU n="236">дьэ,</IU>
    <IU n="237">[атахл-э-ш-тэ ай] —</IU>
    <IU n="238">то эсти,</IU>
    <IU n="239">йаалм-э-ш-тэ-лэ ай кэл-гэ-к мон-и.</IU>
    <IU n="240">(0.5)</IU>
    <IU n="241">аҕа мон-и,</IU>
    <IU n="242">кэл-тэ-йэ.</IU>
    <IU n="243">(1.3)</IU>
    <IU n="244">та-ӈ шоромо,</IU>
    <IU n="245">(0.3)</IU>
    <IU n="246">богатый-ӈоот кудэ-й,</IU>
    <IU n="247">купиэц-ӈоот кудэ-й.</IU>
    <IU n="248">(0.5)</IU>
    <IU n="249">самый самый большой,</IU>
    <IU n="250">купиэц-ӈоот кудэ-й.</IU>
    <IU n="251">(0.7)</IU>
    <IU n="252">та-ӈ иэ,</IU>
    <IU n="253">пушнина мидь-у-л-бэн.</IU>
    <IU n="254">(1.0)</IU>
    <IU n="255">всьэ,</IU>
    <IU n="256">туд-и-н эг-у-жу-ӈи,</IU>
    <IU n="257">вэсь посьолок эг-у-жу-й —</IU>
    <IU n="258">эг-у-жу-й тудэ-л,</IU>
    <IU n="259">(0.4)</IU>
    <IU n="260">потому што,</IU>
    <IU n="261">богатый-ӈоот кудэ-й тудэ-л,</IU>
    <IU n="262">(0.4)</IU>
    <IU n="263">тамун туд-и-н иэ,</IU>
    <IU n="264">...</IU>
    <IU n="265">(0.9)</IU>
    <IU n="266">взад впиэриод эй-рэ-ӈи.</IU>
    <IU n="267">(0.8)</IU>
    <IU n="268">иэ,</IU>
    <IU n="269">дьэ,</IU>
    <IU n="270">(0.3)</IU>
    <IU n="271">та-ӈ шоромо дума-сь.</IU>
    <IU n="272">(1.3)</IU>
    <IU n="273">мэт идьии хон-тэ-йэ,</IU>
    <IU n="274">(0.4)</IU>
    <IU n="275">хон-дэллэ,</IU>
    <IU n="276">льэ-т-чэ.</IU>
    <IU n="277">(1.2)</IU>
    <IU n="278">бочка тадии,</IU>
    <IU n="279">(0.6)</IU>
    <IU n="280">бочка тадии-дэллэ,</IU>
    <IU n="281">(0.3)</IU>
    <IU n="282">тудэ-л имм-уу-тэ-й,</IU>
    <IU n="283">(0.5)</IU>
    <IU n="284">мэт тудэ-л куд-дэ-т.</IU>
    <IU n="285">(1.1)</IU>
    <IU n="286">[куд-дэ-ллэ] —</IU>
    <IU n="287">[куд-дэ-дэ] —</IU>
    <IU n="288">куд-дэ-ӈидэ,</IU>
    <IU n="289">аду-ӈ пушнина-ги чумумидь-у-т.</IU>
    <IU n="290">(1.3)</IU>
    <IU n="291">мон-и,</IU>
    <IU n="292">(0.6)</IU>
    <IU n="293">таа-т чуӈжэ-й.</IU>
    <IU n="294">(1.7)</IU>
    <IU n="295">хон-и,</IU>
    <IU n="296">хон-дэллэ дьэ,</IU>
    <IU n="297">(0.7)</IU>
    <IU n="298">та-ӈ бочка-лэ оожэ-ш-у-м,</IU>
    <IU n="299">(0.6)</IU>
    <IU n="300">чистэ чуму.</IU>
    <IU n="301">(0.4)</IU>
    <IU n="302">о —</IU>
    <IU n="303">саам эл оожаа-ну-й,</IU>
    <IU n="304">(0.8)</IU>
    <IU n="305">дьэ,</IU>
    <IU n="306">(0.3)</IU>
    <IU n="307">модо-й.</IU>
    <IU n="308">(0.4)</IU>
    <IU n="309">...</IU>
    <IU n="310">лосьил йаҕил-гэ-мэ —</IU>
    <IU n="311">модо-до-го,</IU>
    <IU n="312">(1.3)</IU>
    <IU n="313">ньумудьии-лэ,</IU>
    <IU n="314">(0.4)</IU>
    <IU n="315">йоуҕо-му-ӈин,</IU>
    <IU n="316">пай-м тии.</IU>
    <IU n="317">(1.9)</IU>
    <IU n="318">пай-м,</IU>
    <IU n="319">(0.4)</IU>
    <IU n="320">[пай-дэ] —</IU>
    <IU n="321">пай-дэ-гэ,</IU>
    <IU n="322">(0.9)</IU>
    <IU n="323">только пай-м,</IU>
    <IU n="324">(0.4)</IU>
    <IU n="325">ньэ лэмэ эл лэй-дии.</IU>
    <IU n="326">(1.4)</IU>
    <IU n="327">таа-т,</IU>
    <IU n="328">өнмэ-дэ-сь,</IU>
    <IU n="329">(0.6)</IU>
    <IU n="330">[лэбиэ],</IU>
    <IU n="331">(0.6)</IU>
    <IU n="332">пол-гэ лодаах ход-оо-й.</IU>
    <IU n="333">(1.6)</IU>
    <IU n="334">дьэ,</IU>
    <IU n="335">(0.8)</IU>
    <IU n="336">мон-и,</IU>
    <IU n="337">эр-чэ шоромо оо-льэл-дьэк мон-и,</IU>
    <IU n="338">(0.5)</IU>
    <IU n="339">мэт иэ,</IU>
    <IU n="340">ужэ тэт-у-л лэй-тэ-йэ мон-и,</IU>
    <IU n="341">(0.3)</IU>
    <IU n="342">мэт-у-л куд-дэ-л-ӈин,</IU>
    <IU n="343">куд-дэ-ллэ мон-и мэт,</IU>
    <IU n="344">пушнина мин-дин мон-и мэт,</IU>
    <IU n="345">(0.6)</IU>
    <IU n="346">йододьубэ мэт,</IU>
    <IU n="347">льэ,</IU>
    <IU n="348">тэриль мин-дин мэ льэ-льэл-дьэк мон-и,</IU>
    <IU n="349">таа-т,</IU>
    <IU n="350">думайэш —</IU>
    <IU n="351">дума-сьэк мон-и,</IU>
    <IU n="352">(0.6)</IU>
    <IU n="353">тэт мон-и,</IU>
    <IU n="354">саарэӈи оо-льэл-дьэк мон-и.</IU>
    <IU n="355">(0.6)</IU>
    <IU n="356">дурак оо-льэл-дьэк.</IU>
    <IU n="357">(0.8)</IU>
    <IU n="358">мэт-у-л мон-и,</IU>
    <IU n="359">тэтзиэмльа хозиаин,</IU>
    <IU n="360">ханьин  куд-дэ-мэк тэт,</IU>
    <IU n="361">куд-дэ-дин эрдьиэ-йэк мон-и.</IU>
    <IU n="362">(0.7)</IU>
    <IU n="363">мэт мон-и,</IU>
    <IU n="364">тэт-у-л ирк-эй-т,</IU>
    <IU n="365">ирк-э-сьэ мон-и,</IU>
    <IU n="366">(0.8)</IU>
    <IU n="367">ирк-эй-дэллэ мон-и,</IU>
    <IU n="368">тэт-у-л лэу мон-и.</IU>
    <IU n="369">(0.4)</IU>
    <IU n="370">и всьо.</IU>
