  <stext>
    <IU n="1">колхос,</IU>
    <IU n="2">(0.8)</IU>
    <IU n="3">мит-кэлэ командьировка-гэ йад-у-м.</IU>
    <IU n="4">(0.6)</IU>
    <IU n="5">номэр-э йуө-ӈин.</IU>
    <IU n="6">(0.8)</IU>
    <IU n="7">номэр йуө-дэллэ,</IU>
    <IU n="8">ходо-т-ай-дэллэ ходо-т-ай-т йаха-иили,</IU>
    <IU n="9">(0.4)</IU>
    <IU n="10">чугиэ мит-кэлэ шоромо эльэ,</IU>
    <IU n="11">(0.3)</IU>
    <IU n="12">чиргэ-д-эй-м.</IU>
    <IU n="13">(0.5)</IU>
    <IU n="14">йаха-дэллэ йуукэ йаха-дэллэ,</IU>
    <IU n="15">чом-оо-дьэ колхос-кэ,</IU>
    <IU n="16">йаха-дэллэ аасьэ мидь-ии,</IU>
    <IU n="17">(1.0)</IU>
    <IU n="18">полторасто аасьэ-к,</IU>
    <IU n="19">(1.1)</IU>
    <IU n="20">дьэ таа,</IU>
    <IU n="21">(0.7)</IU>
    <IU n="22">приимиэрно,</IU>
    <IU n="23">(0.8)</IU>
    <IU n="24">киндьэ-дэ эймундэ-гэ эй-рэ-ньит,</IU>
    <IU n="25">таа-т кэб-эй-льэл-дьэ.</IU>
    <IU n="26">(1.3)</IU>
    <IU n="27">испиэчиальный проводньик-эк кэй-ӈилэ мит-и-н.</IU>
    <IU n="28">(1.6)</IU>
    <IU n="29">таа-т,</IU>
    <IU n="30">(0.2)</IU>
    <IU n="31">льэ-гэ таӈдиэт молҕаа-н унуӈ-гэн килэ-т,</IU>
    <IU n="32">сьэльсовиэт-кэ йаха-иили.</IU>
    <IU n="33">(0.5)</IU>
    <IU n="34">йообии йуө-дин,</IU>
    <IU n="35">нумун-э нутньэ-йэ нумө-к льэ-л.</IU>
    <IU n="36">(0.5)</IU>
    <IU n="37">тамун чистэ йаха-иили,</IU>
    <IU n="38">(1.2)</IU>
    <IU n="39">наҕаа чом-оо-дьэ пиэ-к,</IU>
    <IU n="40">(0.3)</IU>
    <IU n="41">тамун льэ-ну-л чирчэг-эй-ну-л.</IU>
    <IU n="42">(0.5)</IU>
    <IU n="43">дьэ таа-т йаха-лу-кэ рай,</IU>
    <IU n="44">(0.2)</IU>
    <IU n="45">чугиэ чугө-гэ,</IU>
    <IU n="46">дьэ моӈ-ӈи,</IU>
    <IU n="47">тии йаха-тэ-йли моӈ-ӈи мэт кэнэ-пэ,</IU>
    <IU n="48">(0.3)</IU>
    <IU n="49">интиэриэсный миэстэ-гэ.</IU>
    <IU n="50">(0.9)</IU>
    <IU n="51">да.</IU>
    <IU n="52">(0.3)</IU>
    <IU n="53">чом-оо-дьэ,</IU>
    <IU n="54">чом-оо-дьэ куча-к льэ-л букатын.</IU>
    <IU n="55">(0.8)</IU>
    <IU n="56">разный,</IU>
    <IU n="57">(0.5)</IU>
    <IU n="58">чолҕоро хаар,</IU>
    <IU n="59">дажэ шахалэ-н хаар-эк льэ-л,</IU>
    <IU n="60">(1.0)</IU>
    <IU n="61">патрон разный,</IU>
    <IU n="62">котиэлок,</IU>
    <IU n="63">банка,</IU>
    <IU n="64">кружка,</IU>
    <IU n="65">чайньик,</IU>
    <IU n="66">(0.8)</IU>
    <IU n="67">палкии,</IU>
    <IU n="68">там разный,</IU>
    <IU n="69">вэриовкии,</IU>
    <IU n="70">(1.9)</IU>
    <IU n="71">чумут льэ-й.</IU>
    <IU n="72">(1.8)</IU>
    <IU n="73">чумут льэ-й.</IU>
    <IU n="74">(0.6)</IU>
    <IU n="75">дьэ,</IU>
    <IU n="76">(0.6)</IU>
    <IU n="77">табун-гэлэ мон-и,</IU>
    <IU n="78">тудаа ай таа-т,</IU>
    <IU n="79">мидол-ӈи-дэ-гэ,</IU>
    <IU n="80">ирк-и-н вриэмиа-гэ аасьэ-лэ мит чуөльэ-д омнии саханэ мидол-ӈи-дэ-гэ,</IU>
    <IU n="81">таа-т тиннэй-д-эй-т мад-аа-льэл-ӈи,</IU>
    <IU n="82">модо-ӈи-дэ-гэ,</IU>
    <IU n="83">(0.9)</IU>
    <IU n="84">ирк-и-н бабушка-к уө-ньэ-льэл-у-л.</IU>
    <IU n="85">(0.4)</IU>
    <IU n="86">тэрикэ-к.</IU>
    <IU n="87">(2.3)</IU>
    <IU n="88">дьэ таа льэ-ӈидэ,</IU>
    <IU n="89">таа йуө-ӈи-дэ-гэ рай,</IU>
    <IU n="90">таа-т льэ-ллэ,</IU>
    <IU n="91">атах-у-н уө-ньэ-льэл.</IU>
    <IU n="92">(0.6)</IU>
    <IU n="93">та-ӈ тэрикэ.</IU>
    <IU n="94">(0.9)</IU>
    <IU n="95">йуө-дэ та-ӈ,</IU>
    <IU n="96">(0.6)</IU>
    <IU n="97">аасьэ-ги,</IU>
    <IU n="98">(1.0)</IU>
    <IU n="99">та-ӈ,</IU>
    <IU n="100">(0.2)</IU>
    <IU n="101">тэрикэ,</IU>
    <IU n="102">(0.4)</IU>
    <IU n="103">ходо модо-л таа-т чирэ-й-льэл.</IU>
    <IU n="104">(1.0)</IU>
    <IU n="105">шэльг-э-сь,</IU>
    <IU n="106">(0.8)</IU>
    <IU n="107">чирэ-й-льэл.</IU>
    <IU n="108">(0.6)</IU>
    <IU n="109">дьэ,</IU>
    <IU n="110">(0.4)</IU>
    <IU n="111">уөр-пэ-п-ки йуө-дэллэ,</IU>
    <IU n="112">ходо кудэ мо-ллэ мөд-эй-ӈи,</IU>
    <IU n="113">эл ан-нь-оо-л букатын.</IU>
    <IU n="114">(0.8)</IU>
    <IU n="115">эл ан-нь-аа-ну-й,</IU>
    <IU n="116">ажуу-ги шобольо-л,</IU>
    <IU n="117">нугэн-э льэ-льэл кэрпиэ-льэл.</IU>
    <IU n="118">(0.6)</IU>
    <IU n="119">дьэ,</IU>
    <IU n="120">(0.3)</IU>
    <IU n="121">кэб-эй-ӈи-к мон-и.</IU>
    <IU n="122">(0.7)</IU>
    <IU n="123">дьэ кэб-эй-льэл-ӈи,</IU>
    <IU n="124">(0.7)</IU>
    <IU n="125">дьэ,</IU>
    <IU n="126">атахл-и-дьэ ильи йаалм-и-дьэ аммал-дэ-гэ,</IU>
    <IU n="127">ай кэл-льэл-ӈи.</IU>
    <IU n="128">(0.4)</IU>
    <IU n="129">та-ӈидэ йуө-дин,</IU>
    <IU n="130">аасьэ-ги,</IU>
    <IU n="131">молдҕ,</IU>
    <IU n="132">(0.8)</IU>
    <IU n="133">унму-д иисьэ эгэдэ-й только.</IU>
    <IU n="134">(0.8)</IU>
    <IU n="135">вот,</IU>
    <IU n="136">(1.1)</IU>
    <IU n="137">та-ӈ эмэй-пэ-ги,</IU>
    <IU n="138">оо-льэл,</IU>
    <IU n="139">та-ӈ бабушка,</IU>
    <IU n="140">(0.4)</IU>
    <IU n="141">йуө-дэ молдҕ йоосьо-к,</IU>
    <IU n="142">туөн-пэ-ги только виидно,</IU>
    <IU n="143">(0.2)</IU>
    <IU n="144">[йэд-уу-й],</IU>
    <IU n="145">(1.3)</IU>
    <IU n="146">йэд-уу-й.</IU>
    <IU n="147">(0.5)</IU>
    <IU n="148">дьэ таа-т,</IU>
    <IU n="149">(0.5)</IU>
    <IU n="150">тамун,</IU>
    <IU n="151">(1.0)</IU>
    <IU n="152">лэй-дин шоромо-нь-и,</IU>
    <IU n="153">(0.4)</IU>
    <IU n="154">мит тии льэ-лу-кэ,</IU>
    <IU n="155">миэкэ-ӈин ай эл йуө-дэ аасьэ-н куөкэ мон-и,</IU>
    <IU n="156">(0.3)</IU>
    <IU n="157">унму-ги.</IU>
    <IU n="158">(0.8)</IU>
    <IU n="159">таа-т өйльэ,</IU>
    <IU n="160">(0.4)</IU>
    <IU n="161">туөн,</IU>
    <IU n="162">разно-ӈоо-дьоон,</IU>
    <IU n="163">(1.1)</IU>
    <IU n="164">хам-у-н ньэмолҕи-н,</IU>
    <IU n="165">таҕарии,</IU>
    <IU n="166">куча-к шахальэ-ш-оо-ги,</IU>
    <IU n="167">приимиэрно шоромо-гэ,</IU>
    <IU n="168">ти-ӈ кит-ньит.</IU>
    <IU n="169">(0.8)</IU>
    <IU n="170">такой</IU>
    <IU n="171">(0.2)</IU>
    <IU n="172">куча-лэк,</IU>
    <IU n="173">(0.9)</IU>
    <IU n="174">шахальэ-ш-оо-пэ-ги.</IU>
    <IU n="175">(1.3)</IU>
    <IU n="176">таа-т,</IU>
    <IU n="177">кэб-эй-дэллэ ирк-и-н миэстэ-гэ йаха-иили.</IU>
    <IU n="178">(0.6)</IU>
    <IU n="179">...</IU>
    <IU n="180">(1.5)</IU>
    <IU n="181">протока йалҕил-гэ.</IU>
    <IU n="182">(1.4)</IU>
    <IU n="183">эдуөн ирк-и-н миэстэ-гэ польа-ньэ-й,</IU>
    <IU n="184">(1.3)</IU>
    <IU n="185">таа йаха-дэллэ,</IU>
    <IU n="186">ирк-и-н польа-гэ,</IU>
    <IU n="187">льэ-лэк,</IU>
    <IU n="188">криэс-э-лэк оҕ-оо-л.</IU>
    <IU n="189">(1.7)</IU>
    <IU n="190">та-ӈ криэс ...</IU>
    <IU n="191">... мо-дьэ,</IU>
    <IU n="192">адуөн ай мон-и,</IU>
    <IU n="193">тии ай льэ-лэк льэ-л.</IU>
    <IU n="194">(1.9)</IU>
    <IU n="195">ирк-и-н шоромо,</IU>
    <IU n="196">ирк-и-н,</IU>
    <IU n="197">льэ-ньэ,</IU>
    <IU n="198">ирк-и-н шоромо-ньэ,</IU>
    <IU n="199">(0.6)</IU>
    <IU n="200">мөд-ии-к,</IU>
    <IU n="201">дьикиий шоромо-ньэ,</IU>
    <IU n="102">(1.2)</IU>
    <IU n="203">кимдьии-льэл-ӈи.</IU>
    <IU n="204">(0.5)</IU>
    <IU n="205">та-ӈ кимдьии-т,</IU>
    <IU n="206">та-ӈ шоромо кэй-д-эй-льэл-у-л,</IU>
    <IU n="207">та-ӈ шоромо-гэлэ.</IU>
    <IU n="208">(1.0)</IU>
    <IU n="209">тамуӈ-ӈин памиатньик эгэ-т-оо-пэ-ги,</IU>
    <IU n="210">истэлбаа.</IU>
  </stext>
