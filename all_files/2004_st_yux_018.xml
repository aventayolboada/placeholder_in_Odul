  <stext>
    <IU n="1">мэт хаахаа мэт-кэлэ,</IU>
    <IU n="2">(0.8)</IU>
    <IU n="3">ай,</IU>
    <IU n="4">(4.3)</IU>
    <IU n="5">алмэ ӈоон аа-дин,</IU>
    <IU n="6">(0.4)</IU>
    <IU n="7">гудэ-ль-иэ-льэл.</IU>
    <IU n="8">(1.0)</IU>
    <IU n="9">мэт таҕанэ чом-оо-дьоон оо-дьэ.</IU>
    <IU n="10">(1.4)</IU>
    <IU n="11">мэт-кэт йоулу-сь-у-м.</IU>
    <IU n="12">(0.4)</IU>
    <IU n="13">пойнэ-й чугө-гэн хон-тэ-йэ-к,</IU>
    <IU n="14">ду эмбэ-й чугө-гэн.</IU>
    <IU n="15">(0.7)</IU>
    <IU n="16">мэт,</IU>
    <IU n="17">(0.6)</IU>
    <IU n="18">пойнэ-й чугө,</IU>
    <IU n="19">(1.1)</IU>
    <IU n="20">өнмэ-гэ льэ-т мо-дьэ.</IU>
    <IU n="21">(1.3)</IU>
    <IU n="22">[чиэдьэ] —</IU>
    <IU n="23">чиэдьэ-льэ,</IU>
    <IU n="24">пукэльэ-лэ хон-ну-й чугө-к,</IU>
    <IU n="25">(0.8)</IU>
    <IU n="26">эмбэ-й,</IU>
    <IU n="27">льэ,</IU>
    <IU n="28">(0.8)</IU>
    <IU n="29">пугэ-н льэ чугө-к мон-у-т</IU>
    <IU n="30">(1.6)</IU>
    <IU n="31">мм,</IU>
    <IU n="32">(1.6)</IU>
    <IU n="33">пугэ-л-льэ чугө омо-сь мон-у-т.</IU>
    <IU n="34">иэ</IU>
    <IU n="35">табун мо-дьэ туд-и-н,</IU>
    <IU n="36">(0.7)</IU>
    <IU n="37">эмбэ-й чугө-гэн.</IU>
    <IU n="38">(0.6)</IU>
    <IU n="39">иэ [эр-чэ чугө-к],</IU>
    <IU n="40">(2.0)</IU>
    <IU n="41">эр-чэ чугө-к мин-мэ.</IU>
    <IU n="42">(2.0)</IU>
    <IU n="43">эл талан-ньэ-т-чэ-к.</IU>
    <IU n="44">(0.7)</IU>
    <IU n="45">мад-аа-к абуд-аа-к,</IU>
    <IU n="46">миидьии-гэ мэт-и-н мон-и.</IU>
    <IU n="47">(1.1)</IU>
    <IU n="48">чоҕойо-пул мин-дэллэ,</IU>
    <IU n="49">(0.7)</IU>
    <IU n="50">ирк-иэ-л,</IU>
    <IU n="51">(1.3)</IU>
    <IU n="52">шободэӈиль-гэ.</IU>
    <IU n="53">(0.4)</IU>
    <IU n="54">ирк-иэ-л пудэ лаӈи-дэ,</IU>
    <IU n="55">ирк-иэ-л молҕо лаӈи-дэ шаша-ҕа-д-ай-м.</IU>
    <IU n="56">(0.9)</IU>
    <IU n="57">окно-д аӈиль-пэ-гэн ай таа-т,</IU>
    <IU n="58">(1.2)</IU>
    <IU n="59">ньэ маало-ҕудэ чоҕойо-к пөнь-ии-м.</IU>
    <IU n="60">(0.3)</IU>
    <IU n="61">пуди лаӈи-дэ-т,</IU>
    <IU n="62">молҕо лаӈи-дэ-т иисьэ-ҕа-т.</IU>
    <IU n="63">(0.4)</IU>
    <IU n="64">мит нумө,</IU>
    <IU n="65">(0.8)</IU>
    <IU n="66">луусьии-н нумө,</IU>
    <IU n="67">титэ уй-оо-й,</IU>
    <IU n="68">(0.6)</IU>
    <IU n="69">пуд-дэ-гэн,</IU>
    <IU n="70">(1.1)</IU>
    <IU n="71">урасаа титэ,</IU>
    <IU n="72">(0.7)</IU>
    <IU n="73">пудэбэ-нь-и.</IU>
    <IU n="74">(2.3)</IU>
    <IU n="75">мэт абуд-аа-л-гэ,</IU>
    <IU n="76">(0.6)</IU>
    <IU n="77">мэт будиэ-н,</IU>
    <IU n="78">(0.7)</IU>
    <IU n="79">йашик-пэ-лэк,</IU>
    <IU n="80">(0.6)</IU>
    <IU n="81">доска-пэ-лэк,</IU>
    <IU n="82">(0.8)</IU>
    <IU n="83">пөм-э-сь,</IU>
    <IU n="84">льэ-ӈи,</IU>
    <IU n="85">лон-д-иэ-льэл.</IU>
    <IU n="86">(1.9)</IU>
    <IU n="87">мэт,</IU>
    <IU n="88">(0.7)</IU>
    <IU n="89">йоо пай-ай-дэллэ [йуө-лу-гэ],</IU>
    <IU n="90">(0.5)</IU>
    <IU n="91">йуө-лу-гэ,</IU>
    <IU n="92">(0.9)</IU>
    <IU n="93">шободэӈиль-гэ,</IU>
    <IU n="94">(1.5)</IU>
    <IU n="95">льэ-л,</IU>
    <IU n="96">(0.3)</IU>
    <IU n="97">охно-д аӈиль-гэ льэ-л,</IU>
    <IU n="98">(2.6)</IU>
    <IU n="99">чоҕойо-пул.</IU>
    <IU n="100">(1.6)</IU>
    <IU n="101">титтэ чомоол-э эрэ норхо-ҕо-ну-ӈи.</IU>
    <IU n="102">(0.8)</IU>
    <IU n="103">эл шэй-р-эй-гэ-н мон-у-т.</IU>
    <IU n="104">(0.3)</IU>
    <IU n="105">...</IU>
    <IU n="106">(0.4)</IU>
    <IU n="107">мэт будиэ эмэськэ таа ход-оо-лу-гэ.</IU>
    <IU n="108">(0.7)</IU>
    <IU n="109">чом-оо-дьэ йашик-пэ-лэк ньоҕо-ӈи-л.</IU>
    <IU n="110">(0.4)</IU>
    <IU n="111">мэт-кэлэ таа олмон шэй-рэ-ллэ,</IU>
    <IU n="112">(0.6)</IU>
    <IU n="113">эрбэ-д-эй-л-иэ-ӈи.</IU>
    <IU n="114">(0.7)</IU>
    <IU n="115">мэт чуул-гэ [йаха-дэ] —</IU>
    <IU n="116">йаха-ӈи-дэ-гэ,</IU>
    <IU n="117">тамун өр-ньэ-йэ.</IU>
    <IU n="118">(1.9)</IU>
    <IU n="119">иӈлэ-т.</IU>
    <IU n="120">(0.7)</IU>
    <IU n="121">[таа-т],</IU>
    <IU n="122">(0.5)</IU>
    <IU n="123">таа-т алмэ ӈоот эл гудэ-йэ.</IU>
    <IU n="124">(0.6)</IU>
    <IU n="125">таа эл өр-ньэ-льэл-гэнэ,</IU>
    <IU n="126">(0.3)</IU>
    <IU n="127">таа,</IU>
    <IU n="128">(0.4)</IU>
    <IU n="129">алмэ ӈоот мэ=т гудэ-льэл-дьэ.</IU>
