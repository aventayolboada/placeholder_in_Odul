  <stext>
    <IU n="1">мит участок-кэ модо-т,</IU>
    <IU n="2">(0.9)</IU>
    <IU n="3">чумут ульэгэ көудэ-ну-йли,</IU>
    <IU n="4">сьэмиа-ньэ.</IU>
    <IU n="5">(1.9)</IU>
    <IU n="6">мит молҕаа-д уө оо-дьоон-пэ-лэк иркилльэ.</IU>
    <IU n="7">(1.0)</IU>
    <IU n="8">иркилльэ-н оо-дьоон.</IU>
    <IU n="9">(1.7)</IU>
    <IU n="10">мит-кэт,</IU>
    <IU n="11">(0.3)</IU>
    <IU n="12">мидо-т йук-оо-миэ-дьэ-гэ.</IU>
    <IU n="13">(0.5)</IU>
    <IU n="14">ирк-и-н омнии-к льэ-ӈи-л,</IU>
    <IU n="15">(0.8)</IU>
    <IU n="16">таа йук-оо ирк-и-н көй-диэ,</IU>
    <IU n="17">(0.3)</IU>
    <IU n="18">көй оо-дьэ уө-ньэ-ӈи.</IU>
    <IU n="19">(2.0)</IU>
    <IU n="20">чумут ульэгэ көудэ-дин кэб-эй-нун-ну-ӈи,</IU>
    <IU n="21">(0.6)</IU>
    <IU n="21">мит ньаҕаа та-ӈ уө-ньэ ньэ пөнь-оо-т,</IU>
    <IU n="22">(0.4)</IU>
    <IU n="23">та-ӈ уө.</IU>
    <IU n="24">(1.0)</IU>
    <IU n="25">льэ-т пөнь-аа-ну-нун-дьиили.</IU>
    <IU n="26">(0.6)</IU>
    <IU n="27">йуө-т.</IU>
    <IU n="28">(2.0)</IU>
    <IU n="29">та-ӈ уө эсьиэ-ги,</IU>
    <IU n="30">(2.3)</IU>
    <IU n="31">йуө-лу-гэ уйл-э —</IU>
    <IU n="32">(0.4)</IU>
    <IU n="33">льэ-ллэ көудэ-ллэ чэмэ-й-дэллэ.</IU>
    <IU n="34">(0.6)</IU>
    <IU n="35">ньэ лэмэ аа-т ... эл йуө-т,</IU>
    <IU n="36">(1.3)</IU>
    <IU n="37">эл лэг-у-т,</IU>
    <IU n="38">(1.5)</IU>
    <IU n="39">парнаа,</IU>
    <IU n="40">(1.0)</IU>
    <IU n="41">абут,</IU>
    <IU n="42">(0.3)</IU>
    <IU n="43">абут-эк табуд-э хой-н-аа-л-аа-льэл-у-м.</IU>
    <IU n="44">(0.6)</IU>
    <IU n="45">ужжоо-лэ.</IU>
    <IU n="46">(1.9)</IU>
    <IU n="47">[та-ӈ] —</IU>
    <IU n="48">(1.2)</IU>
    <IU n="49">та-ӈ,</IU>
    <IU n="50">парнаа,</IU>
    <IU n="51">льэ-л абут-ньэ-л,</IU>
    <IU n="52">(0.8)</IU>
    <IU n="53">шаал.</IU>
    <IU n="53">(1.3)</IU>
    <IU n="54">палатка-гэт эл йуукэ оҕ-оо-льэл.</IU>
    <IU n="55">(1.4)</IU>
    <IU n="56">тудэ-л,</IU>
    <IU n="57">(0.7)</IU>
    <IU n="58">парнаа,</IU>
    <IU n="59">уө-гэлэ,</IU>
    <IU n="60">(0.6)</IU>
    <IU n="61">[ной] —</IU>
    <IU n="62">[ной-му-ӈин],</IU>
    <IU n="63">(1.0)</IU>
    <IU n="64">льэ-ллэ,</IU>
    <IU n="65">ной-ги,</IU>
    <IU n="66">(0.7)</IU>
    <IU n="67">йоҕор-т-аа-льэл-мэлэ.</IU>
    <IU n="68">(2.3)</IU>
    <IU n="69">мэт эсьиэ,</IU>
    <IU n="70">эмэй-ньэ ньаҕаа тудэ-гэлэ моӈ-ӈи.</IU>
    <IU n="71">(0.9)</IU>
    <IU n="72">парнаа эл ай-ну-лэ-к ньэ хаин.</IU>
    <IU n="73">(0.8)</IU>
    <IU n="74">ньань-оод-эк.</IU>
    <IU n="75">(0.9)</IU>
    <IU n="76">эди-ӈ льэ,</IU>
    <IU n="77">(0.5)</IU>
    <IU n="78">[эр-чэ-лэк].</IU>
    <IU n="79">(1.6)</IU>
    <IU n="80">[эр-чэ ...].</IU>
    <IU n="81">(0.6)</IU>
    <IU n="82">эр-чоон-гэлэ,</IU>
    <IU n="83">льэ-гэ.</IU>
    <IU n="84">(0.9)</IU>
    <IU n="85">эд-иэ-сь-тэ-м.</IU>
    <IU n="86">(0.8)</IU>
    <IU n="87">та-ӈ,</IU>
    <IU n="88">ной-дэ йоҕор-ньэ-л,</IU>
    <IU n="89">(1.0)</IU>
    <IU n="90">парнаа уө йуө-лу-гэ,</IU>
    <IU n="91">йуө-т мөд-иили.</IU>
    <IU n="92">(0.5)</IU>
    <IU n="93">эдуөн [эр-чоод-эк] —</IU>
    <IU n="94">[эр-чоод-эк].</IU>
    <IU n="95">(1.3)</IU>
    <IU n="96">эр-чоон-гэ кудэ-можуу-ӈи.</IU>
    <IU n="97">(1.7)</IU>
    <IU n="98">йоуло-мэ та-ӈ йук-оо-дьэ уө-гэ.</IU>
    <IU n="99">(0.7)</IU>
    <IU n="100">уө-диэ ной-ги,</IU>
    <IU n="101">пукиэ-ль-иэ-льэл.</IU>
    <IU n="102">(0.3)</IU>
    <IU n="103">та-ӈ эсьиэ-ги,</IU>
    <IU n="104">(1.3)</IU>
    <IU n="105">та-ӈ тудэ уө-гэлэ.</IU>
    <IU n="106">(0.8)</IU>
    <IU n="107">льэ-ӈин посьолк-э-ӈин,</IU>
    <IU n="108">көуд-эй-дин йаха-й.</IU>
  </stext>
