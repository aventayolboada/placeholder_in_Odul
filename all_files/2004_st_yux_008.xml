  <stext>
    <IU n="1">чуөльэ-д омнии саханэ,</IU>
    <IU n="2">(1.6)</IU>
    <IU n="3">поповка,</IU>
    <IU n="4">(1.1)</IU>
    <IU n="5">охотньик-пэ эй-рэ-й-бэн-пэ-к.</IU>
    <IU n="6">(0.5)</IU>
    <IU n="7">та-ӈ эй-рэ-й-ӈи-дэ-гэ,</IU>
    <IU n="8">(0.5)</IU>
    <IU n="9">ирк-и-н пулун-диэ-диэ адуө-ги,</IU>
    <IU n="10">(0.8)</IU>
    <IU n="11">охота-ӈин кэб-э-сь.</IU>
    <IU n="12">(0.6)</IU>
    <IU n="13">[та-ӈ кэб-эй] —</IU>
    <IU n="14">та-ӈ кэб-эй-дэ-гэ,</IU>
    <IU n="15">(1.3)</IU>
    <IU n="16">йуө-й.</IU>
    <IU n="17">(0.2)</IU>
    <IU n="18">йуө-дэ-гэ,</IU>
    <IU n="19">йугул-гэ,</IU>
    <IU n="20">(0.8)</IU>
    <IU n="21">пай-пэ-лэк модо-л,</IU>
    <IU n="22">(0.7)</IU>
    <IU n="23">луусьии-н пай-к,</IU>
    <IU n="24">(0.4)</IU>
    <IU n="25">модо-л.</IU>
    <IU n="26">(0.4)</IU>
    <IU n="27">та-ӈ модо-т,</IU>
    <IU n="28">(0.5)</IU>
    <IU n="29">ади-ӈидэ рассоха лаӈи йуө-дэ,</IU>
    <IU n="30">(0.7)</IU>
    <IU n="31">ади-ӈидэ йуө-дэ рассоха лаӈи,</IU>
    <IU n="32">(0.2)</IU>
    <IU n="33">унмун лаӈи йуө-дэ,</IU>
    <IU n="34">мэ йахт-аа-й.</IU>
    <IU n="35">(0.8)</IU>
    <IU n="36">таа-т,</IU>
    <IU n="37">нугэн-э мил-льэл-у-м,</IU>
    <IU n="38">(0.2)</IU>
    <IU n="39">таа-т,</IU>
    <IU n="40">(0.3)</IU>
    <IU n="41">йахтэ-т мэ модо-й.</IU>
    <IU n="42">таа йахтэ-т модо-до-го,</IU>
    <IU n="43">(0.3)</IU>
    <IU n="44">дума-сь,</IU>
    <IU n="45">(0.6)</IU>
    <IU n="46">хадунгэ-т тудэ шоромо эй-рэ-й-бэд-эк?</IU>
    <IU n="47">(0.9)</IU>
    <IU n="48">пай-пэ эй-рэ-й-бэд-эк туөн,</IU>
    <IU n="49">мэ хадунгэ йаха-й тудэ-л,</IU>
    <IU n="50">интиэриэсно дьэ,</IU>
    <IU n="51">(0.3)</IU>
    <IU n="52">хадунгэ-т йаха-й.</IU>
    <IU n="53">(0.4)</IU>
    <IU n="54">иэ,</IU>
    <IU n="55">мэт тудэ-л,</IU>
    <IU n="56">мэ=т куд-дэ-т.</IU>
    <IU n="57">(1.4)</IU>
    <IU n="58">эг-иэ-й,</IU>
    <IU n="59">эксиль-гэ,</IU>
    <IU n="60">(0.3)</IU>
    <IU n="61">эг-иэ-дэ-гэ давай,</IU>
    <IU n="62">(0.4)</IU>
    <IU n="63">аҕи-дии-м.</IU>
    <IU n="64">(0.3)</IU>
    <IU n="65">ужжоо-лэ мидь-у-м,</IU>
    <IU n="66">аҕи-дии-м таа-т.</IU>
    <IU n="67">(0.3)</IU>
    <IU n="68">хон-и,</IU>
    <IU n="69">хон-и,</IU>
    <IU n="70">хон-дэ-гэ дьэ,</IU>
    <IU n="71">(0.5)</IU>
    <IU n="72">таа-т.</IU>
    <IU n="73">(0.5)</IU>
    <IU n="74">айии-м.</IU>
    <IU n="75">(0.5)</IU>
    <IU n="76">айии-м,</IU>
    <IU n="77">самый,</IU>
    <IU n="78">тии-н молҕо-до-ҕо.</IU>
    <IU n="79">(0.4)</IU>
    <IU n="80">йоуҕо-дэ-гэ айии-м,</IU>
    <IU n="81">(0.2)</IU>
    <IU n="82">тии-н ук-э-сь.</IU>
    <IU n="83">(0.5)</IU>
    <IU n="84">сразу,</IU>
    <IU n="85">(0.3)</IU>
    <IU n="86">пөм-рэ-ш-у-м.</IU>
    <IU n="87">(0.9)</IU>
    <IU n="88">льэ-гэт,</IU>
    <IU n="89">(0.9)</IU>
    <IU n="90">шаал-гэт,</IU>
    <IU n="91">(0.4)</IU>
    <IU n="92">пөм-рэ-ш-тэ-гэ йуө-м,</IU>
    <IU n="93">(1.2)</IU>
    <IU n="94">өйльэ,</IU>
    <IU n="95">ньэ лэмэ өйльэ,</IU>
    <IU n="96">(0.4)</IU>
    <IU n="97">оо,</IU>
    <IU n="98">ха-ӈидэ,</IU>
    <IU n="99">(0.2)</IU>
    <IU n="100">льэ-лэк,</IU>
    <IU n="101">(0.6)</IU>
    <IU n="102">триапка-лэк ход-оо-л только.</IU>
    <IU n="103">(1.0)</IU>
    <IU n="104">шоромо өйльэ,</IU>
    <IU n="105">(0.5)</IU>
    <IU n="106">ньэ амун өйльэ,</IU>
    <IU n="107">(0.1)</IU>
    <IU n="108">ньэ лэмэ өйльэ.</IU>
    <IU n="109">(0.3)</IU>
    <IU n="110">оо дьэ,</IU>
    <IU n="111">(0.9)</IU>
    <IU n="112">льэ мон-и,</IU>
    <IU n="113">(0.6)</IU>
    <IU n="114">истарик мон-и,</IU>
    <IU n="115">(0.5)</IU>
    <IU n="116">тудэ эсьиэ-ги.</IU>
    <IU n="117">(0.8)</IU>
    <IU n="118">оо мон-и,</IU>
    <IU n="119">(0.2)</IU>
    <IU n="120">дурак,</IU>
    <IU n="121">дурак.</IU>
    <IU n="122">(0.8)</IU>
    <IU n="123">ньань-уу-л-бэн машльуө-к айии-мэлэ.</IU>
    <IU n="124">(1.0)</IU>
    <IU n="125">дурак,</IU>
    <IU n="126">(0.3)</IU>
    <IU n="127">ноӈоон мэ айии-м,</IU>
    <IU n="128">(0.7)</IU>
    <IU n="129">лэмэ-д-и-н мэ айии-м,</IU>
    <IU n="130">(0.4)</IU>
    <IU n="131">шаар лэмэ йуө-мэк,</IU>
    <IU n="132">дурак.</IU>
    <IU n="133">(0.8)</IU>
    <IU n="134">дурак ам-дэ-йоо-мэк.</IU>
    <IU n="135">(0.6)</IU>
    <IU n="136">ходо идьии нумө-гэ йаха-й.</IU>
    <IU n="137">(1.1)</IU>
    <IU n="138">тиӈта-ӈ шоромо дьэ йуө-м,</IU>
    <IU n="139">(0.8)</IU>
    <IU n="140">только што,</IU>
    <IU n="141">тудэ льэ-ӈин,</IU>
    <IU n="142">(0.3)</IU>
    <IU n="143">[нумө-ӈин] —</IU>
    <IU n="144">[иэ],</IU>
    <IU n="145">эксиль-ӈин кэб-э-сь,</IU>
    <IU n="145">(0.3)</IU>
    <IU n="147">йоло-ҕудэ долой,</IU>
    <IU n="148">(0.5)</IU>
    <IU n="149">сразу шэльг-э-сь.</IU>
    <IU n="150">(0.8)</IU>
    <IU n="151">давай тудэ эксиль-гэ,</IU>
    <IU n="152">[насьилу] насьилу,</IU>
    <IU n="153">йаха-й.</IU>
    <IU n="154">(0.8)</IU>
    <IU n="155">таа-т ...</IU>
    <IU n="156">(0.1)</IU>
    <IU n="157">йаха-дэллэ —</IU>
    <IU n="158">йаха-дэллэ,</IU>
    <IU n="159">(0.4)</IU>
    <IU n="160">эксиль-гэ дьэ мад-аа-й,</IU>
    <IU n="161">(0.9)</IU>
    <IU n="162">тудэ чомоол-гэ,</IU>
    <IU n="163">(0.5)</IU>
    <IU n="164">само сплавал,</IU>
    <IU n="165">таа-т ход-оо-дэ-гэ таа-т.</IU>
    <IU n="166">(0.9)</IU>
    <IU n="167">эксиль-гэ дьэ,</IU>
    <IU n="168">(0.8)</IU>
    <IU n="169">эксиль-гэ,</IU>
    <IU n="170">льэ-лэк,</IU>
    <IU n="171">(0.2)</IU>
    <IU n="172">алмэ-лэк.</IU>
    <IU n="173">(1.2)</IU>
    <IU n="174">эксиль-гэ.</IU>
    <IU n="175">(0.7)</IU>
    <IU n="176">дьэ тудэ-гэлэ уу-шэ-м.</IU>
    <IU n="177">(1.1)</IU>
    <IU n="178">йалҕи-ньэ-рии-м,</IU>
    <IU n="179">йалҕи-ньэ-рии-м,</IU>
    <IU n="180">таасилэ,</IU>
    <IU n="181">(0.2)</IU>
    <IU n="182">ну,</IU>
    <IU n="183">мон-и.</IU>
    <IU n="184">(1.1)</IU>
    <IU n="185">тэт ньань-уу-л-бэн,</IU>
    <IU n="186">машльуө-к куд-дэ-льэл-мэк,</IU>
    <IU n="187">(0.8)</IU>
    <IU n="188">та-ӈин миэстэ-гэ надо,</IU>
    <IU n="189">(1.0)</IU>
    <IU n="190">памиатньик,</IU>
    <IU n="191">(0.4)</IU>
    <IU n="192">пөнь-ии-ги надо,</IU>
    <IU n="193">(0.4)</IU>
    <IU n="194">шайтан.</IU>
    <IU n="195">(0.8)</IU>
    <IU n="196">памиатньик шайтан называйут.</IU>
    <IU n="197">(0.8)</IU>
    <IU n="198">вот,</IU>
    <IU n="199">(0.2)</IU>
    <IU n="200">тудэ миэстэ-гэн,</IU>
    <IU n="201">(0.5)</IU>
    <IU n="202">тамун,</IU>
    <IU n="203">(0.2)</IU>
    <IU n="204">пөнь-ии-ги надо,</IU>
    <IU n="205">(0.7)</IU>
    <IU n="206">унмун-гэ пөнь-ии-ги.</IU>
    <IU n="207">(0.5)</IU>
    <IU n="208">эгэ-тэ- —</IU>
    <IU n="209">эгэ-тэ-ги надо.</IU>
    <IU n="210">(1.0)</IU>
    <IU n="211">тамун дьэ тудэ-л,</IU>
    <IU n="212">(0.2)</IU>
    <IU n="213">аа-м,</IU>
    <IU n="214">(0.9)</IU>
    <IU n="215">та-ӈ,</IU>
    <IU n="216">памиатньик-э-лэк,</IU>
    <IU n="217">(0.8)</IU>
    <IU n="218">льэ-лэ,</IU>
    <IU n="219">(0.3)</IU>
    <IU n="220">топольа,</IU>
    <IU n="221">(1.0)</IU>
    <IU n="222">хадун льэ-гэ,</IU>
    <IU n="223">угурчэраа-лэ.</IU>
    <IU n="224">(0.5)</IU>
    <IU n="225">аа-дэллэ,</IU>
    <IU n="226">(2.4)</IU>
    <IU n="227">шоромо титэ,</IU>
    <IU n="228">(0.2)</IU>
    <IU n="229">аа-дэллэ,</IU>
    <IU n="230">(0.7)</IU>
    <IU n="231">өрдьэ-дэ-гэ йургуу-ш-у-м,</IU>
    <IU n="232">(0.5)</IU>
    <IU n="233">шаал-гэ мош-шо-м.</IU>
    <IU n="234">(0.5)</IU>
    <IU n="235">тии-нэ,</IU>
    <IU n="236">[шаа-н] —</IU>
    <IU n="237">шаа-н чуул-гэлэ.</IU>
    <IU n="238">(0.4)</IU>
    <IU n="239">штобы та-ӈ,</IU>
    <IU n="240">та-ӈ,</IU>
    <IU n="241">(0.5)</IU>
    <IU n="242">льэ-гэ,</IU>
    <IU n="243">(0.5)</IU>
    <IU n="244">та-ӈ йуө-дэ-дэ-гэ,</IU>
    <IU n="245">(0.3)</IU>
    <IU n="246">та-ӈ пиэ-гэ.</IU>
    <IU n="247">(0.8)</IU>
    <IU n="248">йуө-дэ-дэ-гэ,</IU>
    <IU n="249">(0.3)</IU>
    <IU n="250">йуө-дэ-дэ-гэ аа-м,</IU>
    <IU n="251">(0.7)</IU>
    <IU n="252">тамун-гэлэ,</IU>
    <IU n="253">(0.5)</IU>
    <IU n="254">таа-т аа- —</IU>
    <IU n="255">аа-дэллэ,</IU>
    <IU n="256">бисьэр аӈдьэ-нь-и,</IU>
    <IU n="257">(1.1)</IU>
    <IU n="258">[туөн] —</IU>
    <IU n="259">туөн-пэ-ги чистэ чумут льэ-й.</IU>
    <IU n="260">(0.1)</IU>
    <IU n="261">шоромо-дэ аӈа-пэ-ги,</IU>
    <IU n="262">аӈдьэ-п-ки чистэ чумут,</IU>
    <IU n="263">(0.2)</IU>
    <IU n="264">чумут льэ-й.</IU>
    <IU n="265">(0.3)</IU>
    <IU n="266">унэмэ-п-ки чистэ чумут льэ-й.</IU>
    <IU n="267">(0.6)</IU>
    <IU n="268">вот,</IU>
    <IU n="269">(0.7)</IU>
    <IU n="270">дьэ,</IU>
    <IU n="271">льэ-гэ,</IU>
    <IU n="272">(0.6)</IU>
    <IU n="273">унмун-гэ,</IU>
    <IU n="274">(1.1)</IU>
    <IU n="275">... пиэ-гэ.</IU>
    <IU n="276">(1.1)</IU>
    <IU n="277">эгэ-т-иэ-льэл.</IU>
    <IU n="278">(0.5)</IU>
    <IU n="279">приамо на [биэриэк] —</IU>
    <IU n="280">биэриэк-кэ оҕ-оо-й.</IU>
    <IU n="281">(0.6)</IU>
    <IU n="282">биэриэк-кэ шаал.</IU>
    <IU n="283">(0.5)</IU>
    <IU n="284">та-ӈ лархул-ги оожии-гэлэ,</IU>
    <IU n="285">тудэ-л вон,</IU>
    <IU n="286">цэльиком чумут эгэдэ-й.</IU>
    <IU n="287">(0.6)</IU>
    <IU n="288">кругом,</IU>
    <IU n="289">льэ-лэк,</IU>
    <IU n="290">(1.1)</IU>
    <IU n="291">шөйл-эк.</IU>
    <IU n="292">(1.0)</IU>
    <IU n="293">ну всьо равно,</IU>
    <IU n="294">эл лон-дэ.</IU>
    <IU n="295">(0.6)</IU>
    <IU n="296">тамун-гэлэ,</IU>
    <IU n="297">(0.4)</IU>
    <IU n="298">...</IU>
    <IU n="299">(0.5)</IU>
    <IU n="300">тамун—</IU>
    <IU n="301">тамун-гэлэ экспиэдицийа-пэ миӈ-ӈам.</IU>
    <IU n="302">(0.7)</IU>
    <IU n="303">мин-дэ-гэ көуд-эй-ӈам.</IU>
    <IU n="304">(0.9)</IU>
    <IU n="305">шаал оҕ-оо-й,</IU>
    <IU n="306">(0.8)</IU>
    <IU n="307">это,</IU>
    <IU n="308">(1.0)</IU>
    <IU n="309">льэ-ги өйльэ.</IU>
    <IU n="310">(0.8)</IU>
    <IU n="311">и всьо.</IU>
  </stext>
