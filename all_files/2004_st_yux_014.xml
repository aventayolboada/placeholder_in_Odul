    <IU n="359">чэйлуу-киэ-й эл оож-оо-т,</IU>
    <IU n="360">тамун-гэт таа-т гудэ-йэ.</IU>
    <IU n="361">(0.8)</IU>
    <IU n="362">эдь-и,</IU>
    <IU n="363">(0.8)</IU>
    <IU n="364">тудэ табаах-кэлэ,</IU>
    <IU n="365">айии мой-м,</IU>
    <IU n="366">нугэн-гэ,</IU>
    <IU n="367">(0.4)</IU>
    <IU n="368">...</IU>
    <IU n="369">(0.8)</IU>
    <IU n="370">дьэ таа-т эдь-и мон-у-т,</IU>
    <IU n="371">(0.8)</IU>
    <IU n="372">йумм-уу-йэ,</IU>
    <IU n="373">мон-дэ-гэ,</IU>
    <IU n="374">(1.5)</IU>
    <IU n="375">тамун-гэ мэт —</IU>
    <IU n="376">(0.5)</IU>
    <IU n="377">льэ,</IU>
    <IU n="378">мэт ниӈиэ омо-лбэ-й.</IU>
    <IU n="379">(1.1)</IU>
    <IU n="380">дьэ айии табаах тадии,</IU>
    <IU n="381">тамун-гэлэ-дэ оожэ-м.</IU>
    <IU n="382">(0.8)</IU>
    <IU n="383">мэт чай,</IU>
    <IU n="384">льэ,</IU>
    <IU n="385">оожэ,</IU>
    <IU n="386">лэӈ-дэ-йэ.</IU>
    <IU n="387">(0.3)</IU>
    <IU n="388">чэмэ-рэй-дэллэ,</IU>
    <IU n="389">(0.7)</IU>
    <IU n="390">дьэ таа-т,</IU>
    <IU n="391">(0.5)</IU>
    <IU n="392">кэб-эй-дин кудэ-лэ-йэ.</IU>
    <IU n="393">(0.4)</IU>
    <IU n="394">та-ӈ мэт пулут таа-т,</IU>
    <IU n="395">пөнь-оо-й.</IU>
    <IU n="396">(1.0)</IU>
    <IU n="397">та-ӈ мэт шоромо-пул-гэ йаха-йэ,</IU>
    <IU n="398">таа-т,</IU>
    <IU n="399">(0.3)</IU>
    <IU n="400">өйльэ.</IU>
    <IU n="401">(0.6)</IU>
    <IU n="402">тамун эл лэй-дии-йэ,</IU>
    <IU n="403">чом-оо-н йук-оо-дьоон оо-дьэ.</IU>
    <IU n="404">(0.8)</IU>
    <IU n="405">куньэль ньэмолҕил ду,</IU>
    <IU n="406">куньэль иркии будиэ ньэмолҕил-ньэ-й,</IU>
    <IU n="407">льэ-й-бэн оо-дьэ,</IU>
    <IU n="408">та-ӈ параа-гэ.</IU>
    <IU n="409">(0.7)</IU>
    <IU n="410">йуө-мик,</IU>
    <IU n="411">(0.6)</IU>
    <IU n="412">табаах,</IU>
    <IU n="413">таа-т,</IU>
    <IU n="414">(1.4)</IU>
    <IU n="415">шоромо-лэ нугу-сьии-нун-ну-м,</IU>
    <IU n="416">таа-т эр-уу-льэл,</IU>
    <IU n="417">(0.2)</IU>
    <IU n="418">табаах оожэ-л.</IU>
    <IU n="419">(0.7)</IU>
    <IU n="429">таӈдаа иӈл-оо-л.</IU>
    <IU n="421">(0.6)</IU>
    <IU n="422">табуд-э дьэ чэм-иэ-сь.</IU>
