  <stext>
    <IU n="1">мит чуму сьэмиа-ньэ,</IU>
    <IU n="2">(1.3)</IU>
    <IU n="3">оттуркөл мон-у-т лэбиэ-гэ [оо-дьоон-пэ] —</IU>
    <IU n="4">оо-дьоон-пэ оо-дьиили,</IU>
    <IU n="5">(0.8)</IU>
    <IU n="6">мидо-сьии-ньут.</IU>
    <IU n="7">(2.7)</IU>
    <IU n="8">[мэт атах-у-н мэт чаачаа-гэт].</IU>
    <IU n="9">(2.0)</IU>
    <IU n="10">мэт атах-у-н,</IU>
    <IU n="11">(0.3)</IU>
    <IU n="12">чаачаа эрэ,</IU>
    <IU n="13">(0.9)</IU>
    <IU n="14">льэ-гэ луусьии-н нумө-гэ модо-ӈи.</IU>
    <IU n="15">(0.5)</IU>
    <IU n="16">ирк-и-дьэ —</IU>
    <IU n="17">(0.8)</IU>
    <IU n="18">ирк-и-н угуйэ-л-мэ эмэй эг-иэ-дэллэ,</IU>
    <IU n="19">(0.6)</IU>
    <IU n="20">йуө-м,</IU>
    <IU n="21">(0.6)</IU>
    <IU n="22">ньиэр лэу-ну-л,</IU>
    <IU n="23">(1.1)</IU>
    <IU n="24">йукуньэйэ мон-у-т,</IU>
    <IU n="25">таа-т-миэ,</IU>
    <IU n="26">(1.6)</IU>
    <IU n="27">шөйльбул-эк.</IU>
    <IU n="28">(3.8)</IU>
    <IU n="29">шөйльбул-эк,</IU>
    <IU n="30">табуд-эк нул-льэл-мэлэ,</IU>
    <IU n="31">(2.0)</IU>
    <IU n="32">тамун ньиэр лэг-у-м.</IU>
    <IU n="33">(0.7)</IU>
    <IU n="34">йоӈжоодьэ оо-гэ-н,</IU>
    <IU n="35">(0.2)</IU>
    <IU n="36">молодьо оо-гэ-н,</IU>
    <IU n="37">(0.3)</IU>
    <IU n="38">мурэ оо-гэ-н,</IU>
    <IU n="39">табуд-эк лэу-ну-льэл-мэлэ.</IU>
    <IU n="40">(0.4)</IU>
    <IU n="41">табуд-эк нул-льэл-мэлэ,</IU>
    <IU n="42">(2.0)</IU>
    <IU n="43">эмэй [мон-ну-й] —</IU>
    <IU n="44">мон-нун-ну-й-бэд-эк,</IU>
    <IU n="45">(0.4)</IU>
    <IU n="46">эдуөн эр-чоон льэ-ӈин таа льэ-й.</IU>
    <IU n="47">(1.3)</IU>
    <IU n="48">эди-ӈ льэ,</IU>
    <IU n="49">(0.9)</IU>
    <IU n="50">йукуньэйэ мин-дэ-гэ,</IU>
    <IU n="51">(1.1)</IU>
    <IU n="52">льэ-ги надо.</IU>
    <IU n="53">(2.0)</IU>
    <IU n="54">куд-дэ-ги надо.</IU>
    <IU n="55">(1.3)</IU>
    <IU n="56">эмэй тамун-гэлэ,</IU>
    <IU n="57">(0.7)</IU>
    <IU n="58">та-ӈ шөйльбул-гэлэ,</IU>
    <IU n="59">(0.9)</IU>
    <IU n="60">мил-льэл-у-м.</IU>
    <IU n="61">(0.3)</IU>
    <IU n="62">бочонок молҕо-до-ҕо.</IU>
    <IU n="63">(0.3)</IU>
    <IU n="64">лөуд-уу-льэл.</IU>
    <IU n="65">(0.6)</IU>
    <IU n="66">эмэй айаа-дэллэ,</IU>
    <IU n="67">(0.3)</IU>
    <IU n="68">тамун льэ,</IU>
    <IU n="69">(0.8)</IU>
    <IU n="70">[эр-чоон-гэн ...],</IU>
    <IU n="71">(0.6)</IU>
    <IU n="72">эр-чоон-гэн,</IU>
    <IU n="73">(0.3)</IU>
    <IU n="74">тамун,</IU>
    <IU n="75">(0.5)</IU>
    <IU n="76">[лэу-ну-м мон-у-т].</IU>
    <IU n="77">(1.1)</IU>
    <IU n="78">лэу-ну-м мон-у-т.</IU>
    <IU n="79">(2.0)</IU>
    <IU n="80">табуд-эк мин-ну-мэлэ.</IU>
    <IU n="81">эр-чоон өйльэ-гэ мон-у-т айаа-льэл,</IU>
    <IU n="82">(1.0)</IU>
    <IU n="83">мидэдьэ-лэк таасилэ,</IU>
    <IU n="84">[ирк-и-н] —</IU>
    <IU n="85">ирк-и-н иӈдьии-лэк мил-льэл-мэлэ.</IU>
    <IU n="86">(0.4)</IU>
    <IU n="87">молодьо-ги моро-дэллэ,</IU>
    <IU n="88">(0.4)</IU>
    <IU n="89">та-ӈ шөйльбул-эк.</IU>
    <IU n="90">(0.2)</IU>
    <IU n="91">мил-льэл-мэлэ.</IU>
    <IU n="92">(0.7)</IU>
    <IU n="93">тамун йуө-т оҕ-оо-иили эмэй мин-дэ-гэ,</IU>
    <IU n="94">(0.3)</IU>
    <IU n="95">нуө-дэ,</IU>
    <IU n="96">(0.4)</IU>
    <IU n="97">йуө-дин [ходо] —</IU>
    <IU n="98">(0.3)</IU>
    <IU n="99">ходо [наказ] —</IU>
    <IU n="100">наказывай-м.</IU>
    <IU n="101">(1.4)</IU>
    <IU n="102">эмэй,</IU>
    <IU n="103">(3.8)</IU>
    <IU n="104">анта-ӈ йукуньэйэ аӈа-д-и-н,</IU>
    <IU n="105">(0.5)</IU>
    <IU n="106">мэдин ходо,</IU>
    <IU n="107">(2.8)</IU>
    <IU n="108">мин-дэ ньит-ньэ-й мидэдьэ-гэлэ,</IU>
    <IU n="109">(0.2)</IU>
    <IU n="110">[эл йох-тэ] —</IU>
    <IU n="111">(2.4)</IU>
    <IU n="112">эл йуукэ-нэ гудэ-м.</IU>
    <IU n="113">(1.4)</IU>
    <IU n="114">[ии-дэ-дин] —</IU>
    <IU n="115">аӈа ии-дэ-дин.</IU>
    <IU n="116">(2.0)</IU>
    <IU n="117">эмэськэ та-ӈ гудэ-миэ-гэ,</IU>
    <IU n="118">(2.2)</IU>
    <IU n="119">[ной-лэ уй-ай] —</IU>
    <IU n="120">(0.3)</IU>
    <IU n="121">[уй-ай-дэллэ],</IU>
    <IU n="122">(1.2)</IU>
    <IU n="123">[пулдьэ-гэ-д-эй-льэ-ллэ] —</IU>
    <IU n="124">ной-лэ уй-ай-дэ-гэ пулдьэ-гэ-д-эй-льэ-ллэ,</IU>
    <IU n="125">(0.3)</IU>
    <IU n="126">эмэй ирк-эй-т.</IU>
    <IU n="127">(1.1)</IU>
    <IU n="128">та-ӈ шөйльбул таӈдиэт шэй-р-эй-льэл.</IU>
    <IU n="129">(1.0)</IU>
    <IU n="130">эмэй,</IU>
    <IU n="131">(1.0)</IU>
    <IU n="132">наҕаа,</IU>
    <IU n="133">(1.4)</IU>
    <IU n="134">льэ-й-бэд-эк.</IU>
    <IU n="135">(0.6)</IU>
    <IU n="136">ниӈиэ-ги,</IU>
    <IU n="137">(0.5)</IU>
    <IU n="138">эр-у-лэ-й-бэд-эк.</IU>
    <IU n="139">(0.9)</IU>
    <IU n="140">аӈа-ги [эл ] —</IU>
    <IU n="141">эл ии-дэ-йэ мон-у-т.</IU>
    <IU n="142">(0.5)</IU>
    <IU n="143">пугэ-мэ эр-чоон-гэ йаха-иили,</IU>
    <IU n="144">(0.6)</IU>
    <IU n="145">мэт льэ-пул,</IU>
    <IU n="146">(0.8)</IU>
    <IU n="147">чаачаа-пэ атахл-оо оожии-гэ өйльэ-ӈи.</IU>
    <IU n="148">(2.4)</IU>
    <IU n="149">таӈдиэт,</IU>
    <IU n="150">(0.6)</IU>
    <IU n="151">пооро-мэ,</IU>
    <IU n="152">(0.7)</IU>
    <IU n="153">[та-ӈ] —</IU>
    <IU n="154">(1.2)</IU>
    <IU n="155">та-ӈ шөйльбул,</IU>
    <IU n="156">(0.6)</IU>
    <IU n="157">уӈжоодьэ молҕо-до-ҕо,</IU>
    <IU n="158">таа уө-ньэ-льэл.</IU>
    <IU n="159">(1.7)</IU>
    <IU n="160">та-ӈ,</IU>
    <IU n="161">(0.2)</IU>
    <IU n="162">эмэй-пэ-ги шэй-р-эй-льэл,</IU>
    <IU n="163">(0.3)</IU>
    <IU n="164">шөйльбул эмэй-пэ-ги,</IU>
    <IU n="165">(0.5)</IU>
    <IU n="166">та-ӈ,</IU>
    <IU n="167">(0.5)</IU>
    <IU n="168">уөр-пэ-п-ки оожии-гэ пэйжии-сь.</IU>
    <IU n="169">(0.9)</IU>
    <IU n="170">тии-нэ уӈж-оод-эк.</IU>
    <IU n="171">(0.4)</IU>
    <IU n="172">ноӈоон оожии-гэ пэйжии-мэ,</IU>
    <IU n="173">(1.3)</IU>
    <IU n="174">эдуөн эл куд-дэ-сьуөн.</IU>
    <IU n="175">(0.3)</IU>
    <IU n="176">эл лэй-дии-йэ,</IU>
    <IU n="177">(0.6)</IU>
    <IU n="178">эдуөн эр-чоон-гэ [йаха] —</IU>
    <IU n="179">йаха-д-ай-дэ-гэнэ мон-у-т.</IU>
    <IU n="180">(0.5)</IU>
    <IU n="181">тамун-гэт [мит] —</IU>
    <IU n="182">(0.6)</IU>
    <IU n="183">[мэт] —</IU>
    <IU n="184">(0.7)</IU>
    <IU n="185">таасилэ,</IU>
    <IU n="186">(0.5)</IU>
    <IU n="187">мэт льэ-пул,</IU>
    <IU n="188">(1.3)</IU>
    <IU n="189">паабаа-пэ чумут,</IU>
    <IU n="190">(0.6)</IU>
    <IU n="191">эдуөн-гэлэ өнмэ-гэ льэ-ӈаа,</IU>
    <IU n="192">(0.4)</IU>
    <IU n="193">та-ӈ биэрэ-л-ай-ӈаа.</IU>
  </stext>
